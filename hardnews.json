rl":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"samuel-alemao","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#97;&#109;&#101;&#108;&#97;&#109;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/04/23/local/noticia/monumento-vasco-goncalves-desenhado-siza-alameda-limbo-2088006","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Lisboa","url":"https://www.publico.pt/2024/04/23/local/noticia/monumento-vasco-goncalves-desenhado-siza-alameda-limbo-2088006"},"id":2088006,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Moreira não deu sinais de querer ser cabeça de lista. Mas “o convite não existia”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1902714?tp=UH&db=IMAGENS&type=JPG","author":[{"id":148,"nome":"Margarida Gomes","descricao":"","email":"mgomes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"margarida-gomes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"margarida gomes.JPG","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1262965?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#109;&#111;&#103;&#109;","contribuicao":"","tipo":"INTERNO"},{"id":169,"nome":"Leonete Botelho","descricao":"O jornalismo surgiu como a melhor forma possível - e desejada - de declarar independência ao terminar Direito. O Diário de Coimbra ficava ali na Rua da Sofia, entre o tribunal e o escritório de advogados onde fazia estágio, mas rapidamente o jornalismo se sobrepôs. O prazer da escrita, o resultado imediato, a realidade a acontecer sob os nossos olhos em primeira mão. Viver para contá-la. Voltei à Faculdade quando foi criada a pós-graduação em Direito da Comunicação. Seguiu-se a “tarimba” e o PÚBLICO: três anos correspondente em Coimbra, nove anos na secção de Sociedade no Porto – justiça, migrações, menores, violência doméstica: minorias. Em 2006 rumei a Lisboa e às maiorias: a Política vista da Assembleia da República. Editar foi um desafio que se impôs, outra vez. Mas é na ponta dos dedos, na veia crítica, que o jornalismo fervilha.","email":"lbotelho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editora de Política","profissaoNaAltura":"","slug":"leonete-botelho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"leonete-botelho-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1740630?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#101;&#116;&#111;&#98;&#108;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/04/23/politica/noticia/moreira-nao-deu-sinais-querer-cabeca-lista-convite-nao-existia-2088084","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PSD","url":"https://www.publico.pt/2024/04/23/politica/noticia/moreira-nao-deu-sinais-querer-cabeca-lista-convite-nao-existia-2088084"},"id":2088084,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Sebastião Bugalho: primeiro estranha-se, depois entranha-se","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1902634?tp=UH&db=IMAGENS&type=JPG","author":[{"id":399,"nome":"Luciano Alvarez","descricao":"","email":"luciano.alvarez@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localiz
ida.","email":"apc@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"alexandra-prado-coelho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Alexandra Prado Coelho z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1526725?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#99;&#112;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":24,"url":"/2025/04/09/fugas/reportagem/cafe-sao-bento-chega-cascais-famoso-bife-2129087","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"fugas"},"brand":null,"rubric":{"name":"Reportagem","url":"/gastronomia"},"id":2129087,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

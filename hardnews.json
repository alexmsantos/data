[{"pos":0,"title":"Concurso para recrutar técnicos superiores já tem 12.500 candidaturas","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1855184?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3150,"nome":"Raquel Martins","descricao":"Aos 15 anos decidi que queria mudar o mundo e, para desgosto da minha professora de Português, quis ser jornalista. Fiz o curso de Ciências da Comunicação na Universidade Nova, frequentei o Cenjor e andei indecisa entre a rádio e a imprensa. Depois de uma breve passagem pela rádio (Antena 3 e Rádio Expo), acabei por entrar no jornalismo económico um pouco por acaso. Primeiro, no Diário Económico – onde escrevi sobre ambiente, autarquias, política e, mais tarde, sobre trabalho e função pública – depois no Jornal de Negócios. Foi por isso que, em 2009, fiz uma formação na Universidade Católica sobre legislação laboral. Estou no PÚBLICO desde 2010. Acompanho os assuntos relacionados com trabalho, emprego e administração pública (há quem diga, com humor, que o Código do Trabalho é o meu livro de cabeceira.) Defendo que o jornalismo tem um papel cada vez mais importante na descodificação da avalanche de informação que todos os dias circula nas mais diversas plataformas.","email":"raquel.martins@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"raquel-martins","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#110;&#105;&#116;&#114;&#97;&#109;&#46;&#108;&#101;&#117;&#113;&#97;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/01/economia/noticia/concurso-recrutar-tecnicos-superiores-ja-12500-candidaturas-2068622","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Administração Pública","url":"https://www.publico.pt/2023/11/01/economia/noticia/concurso-recrutar-tecnicos-superiores-ja-12500-candidaturas-2068622"},"id":2068622,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"Inovação portuguesa no abastecimento móvel de hidrogénio","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1853258?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2023/10/26/estudiop/noticia/inovacao-portuguesa-abastecimento-movel-hidrogenio-2067869","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"PRF","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1853253?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2023/10/26/estudiop/noticia/inovacao-portuguesa-abastecimento-movel-hidrogenio-2067869"},"id":2067869,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":2,"title":"Passagem de Rafah será aberta para feridos graves","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1855397?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11214,"nome":"Sofia Neves","descricao":"","email":"sofia.neves@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sofia-neves","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#118;&#101;&#110;&#46;&#97;&#105;&#102;&#111;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/01/mundo/noticia/passagem-rafah-sera-aberta-feridos-graves-portadores-passaportes-estrangeiros-2068699","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Israel","url":"https://www.publico.pt/2023/11/01/mundo/noticia/passagem-rafah-sera-aberta-feridos-graves-portadores-passaportes-estrangeiros-2068699"},"id":2068699,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":3,"title":"Consumo de droga 
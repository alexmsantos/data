leiro@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"diogo-cavaleiro","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#114;&#105;&#101;&#108;&#97;&#118;&#97;&#99;&#46;&#111;&#103;&#111;&#105;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/06/25/economia/noticia/tesla-regista-novo-rombo-vendas-carros-europa-2137766","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Automóveis","url":"https://www.publico.pt/2025/06/25/economia/noticia/tesla-regista-novo-rombo-vendas-carros-europa-2137766"},"id":2137766,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":28,"title":"Três projectos estão a mudar a educação - o seu pode ser o próximo","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2005829?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":90,"url":"/2025/06/09/estudiop/noticia/tres-projectos-estao-mudar-educacao-proximo-2135647","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Sonae","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1693523?tp=UH&db=IMAGENS","slug":"sonae","url":"https://sonae.pt/"},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2025/06/09/estudiop/noticia/tres-projectos-estao-mudar-educacao-proximo-2135647"},"id":2135647,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Casamento Jeff Bezos & Lauren Sánchez: o vestido, o custo da festa, os convidados","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2010377?tp=UH&db=IMAGENS&type=JPG","author":[{"id":890,"nome":"Reuters","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","s
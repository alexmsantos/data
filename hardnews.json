ull,"size":30},{"pos":28,"title":"Empresa na Albânia de directores da Santa Casa na mira das autoridades","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1945614?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11964,"nome":"Sónia Trigueirão","descricao":"","email":"sonia.trigueirao@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sonia-trigueirao","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#97;&#114;&#105;&#101;&#117;&#103;&#105;&#114;&#116;&#46;&#97;&#105;&#110;&#111;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/16/sociedade/noticia/empresa-albania-directores-santa-casa-mira-autoridades-2107542","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Santa Casa da Misericórdia","url":"https://www.publico.pt/2024/10/16/sociedade/noticia/empresa-albania-directores-santa-casa-mira-autoridades-2107542"},"id":2107542,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Itália mandou os primeiros requerentes de asilo para a Albânia – alguns por engano","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1947185?tp=UH&db=IMAGENS&type=JPG","author":[{"id":906,"nome":"Sofia Lorena","descricao":"Uma noite, em Janeiro de 1991, a minha mãe apanhou-me a escrever Saddam Hussein numa folha de papel, olhos colados ao ecrã da televisão. Estranhou. Eu estava na preparatória e colaborava no Sinal Mais. Pareceu-me importante saber escrever o nome do homem que estava em todas as notícias. Imagino que estivesse fascinada com a importância que era dada a algo que me parecia estar a acontecer noutro mundo. Aos poucos, percebi que o mundo é só um e que uma decisão tomada em Washington pode mudar para sempre a vida de alguém em Bagdad. Em 2003, estava no Iraque quando Saddam foi capturado. Em 2010, regressei para uma série de reportagens (Prémio Gazeta). Vi guerras e tive a imensa sorte de ver revoluções. Nunca mais parei de fazer do outro mundo o meu. E até aprendi a escrever Saddam em árabe.","email":"slorena@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sofia-lorena","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"sofia-lorena-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739860?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#101;&#114;&#111;&#108;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/16/mundo/noticia/italia-mandou-requerentes-asilo-albania-engano-2108224","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Migrações","url":"https://www.publico.pt/2024/10/16/mundo/noticia/italia-mandou-requerentes-asilo-albania-engano-2108224"},"id":2108224,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

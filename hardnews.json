.","email":"joao.pincha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"joao-pedro-pincha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Joao P Pincha.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1332529?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#104;&#99;&#110;&#105;&#112;&#46;&#111;&#97;&#111;&#106;","contribuicao":"","tipo":"INTERNO"},{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2025/07/25/mundo/noticia/130-mil-deslocados-dois-dias-confrontos-tailandia-camboja-2141753","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Ásia","url":"https://www.publico.pt/2025/07/25/mundo/noticia/130-mil-deslocados-dois-dias-confrontos-tailandia-camboja-2141753"},"id":2141753,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":15,"title":"Fuze Tea: um passatempo para o ajudar a relaxar","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2015643?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"https://www.publico.pt/estudiop/conteudo_patrocinado/fuze-tea-passatempo","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Conteúdo Comercial","url":"/estudiop"},"id":27086,"type":"EXTERNOS","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Chuvas equivalentes a quase um ano desabaram num dia sobre o Norte da China","isPremium":false,"image":"https:
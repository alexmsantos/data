 jantar, e vibro com a política e as políticas que mudam o nosso mundo.","email":"ruben.martins@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"ruben-martins","imagem":{"w":null,"h":null,"largura":1218,"altura":1118,"nome":"FotoRubenMartins.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1732987?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#110;&#105;&#116;&#114;&#97;&#109;&#46;&#110;&#101;&#98;&#117;&#114;","contribuicao":"","tipo":"INTERNO"},{"id":4123,"nome":"João Ruela Ribeiro","descricao":"","email":"joao.ruela@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"joao-ruela-ribeiro","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#108;&#101;&#117;&#114;&#46;&#111;&#97;&#111;&#106;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/21/mundo/noticia/javier-milei-homem-quer-tornar-argentina-eua-30-anos-2070839","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Podcast P24","url":"https://www.publico.pt/2023/11/21/mundo/noticia/javier-milei-homem-quer-tornar-argentina-eua-30-anos-2070839"},"id":2070839,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Comida de conforto modernizada, no próximo webinar Entre Chefs","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1858570?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2023/11/15/estudiop/noticia/comida-conforto-modernizada-proximo-webinar-chefs-2070059","comments":0,"spoonsored":true,"isOpinion
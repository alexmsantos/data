icao":"","email":"augusto.bernardino@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"augusto-bernardino","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#110;&#105;&#100;&#114;&#97;&#110;&#114;&#101;&#98;&#46;&#111;&#116;&#115;&#117;&#103;&#117;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/06/20/desporto/cronica_de_jogo/hjulmand-mantem-dinamarca-corrida-adia-apuramento-inglaterra-2094761","comments":2,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Crónica de jogo","url":"/futebol-internacional"},"id":2094761,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":23,"title":"Diogo Jota garante estar bem fisicamente e pronto a ajudar","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1918162?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/06/20/desporto/noticia/diogo-jota-garante-estar-bem-fisicamente-pronto-ajudar-2094766","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Futebol internacional","url":"https://www.publico.pt/2024/06/20/desporto/noticia/diogo-jota-garante-estar-bem-fisicamente-pronto-ajudar-2094766"},"id":2094766,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":24,"title":"Jovic salvou a Sérvia dos zero pontos","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1918108?tp=UH&db=IMAGENS&type=JPG","author":[{"id":206,"nome":"Marco Vaza","descricao":"O desporto tem vencedores e perdedores, heróis e vilões e a história nem sempre é sobre o que ganha. Eu escrevo sobre aquele que marca o golo decisivo, sobre aquele que corre mais do que os outros, sobre aquele que não desiste ou sobre aquele que desistiu quando estava perto do fim. Já assisti ao vivo a grandes goleadas, recordes do mundo, estreias de futuras estrelas e despedidas de mitos. Entrei no PÚBLICO como estagiário em 1999 para a secção de Desporto, onde continuo, depois de uma comissão entre 2006 e 2009 como editor da revista de domingo. Para além de centenas de jogos de futebol, já estive em duas edições dos Jogos Olímpicos (Atenas 2004 e Londres 2012), numa edição dos Mundiais de atletismo (Helsínquia 2005) e noutras competições de muitas modalidades. O desporto dos outros é a minha profissão.","email":"mvaza@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"marco-vaza","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Marco_Vaza.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737861?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#122;&#97;&#118;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":26,"url":"/2024/06/20/desporto/noticia/jovic-salvou-servia-zero-pontos-2094728","comments":2,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Euro 2024","url":"https://www.publico.pt/2024/06/20/desporto/noticia/jovic-salvou-servia-zero-pontos-2094728"},"id":2094728,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":25,"title":"Sérvia ameaça abandonar o Euro 2024","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1918106?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","de
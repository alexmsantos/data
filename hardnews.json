gia e ambiente são outros temas da minha área de interesses.","email":"dandrade@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"david-andrade","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"david-andrade-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1849770?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Arminia Bielefeld","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#101;&#100;&#97;&#114;&#100;&#110;&#97;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/20/desporto/noticia/italia-republica-checa-eslovenia-tambem-garantem-presenca-euro-2024-2070932","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Futebol","url":"https://www.publico.pt/2023/11/20/desporto/noticia/italia-republica-checa-eslovenia-tambem-garantem-presenca-euro-2024-2070932"},"id":2070932,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":19,"title":"“Diploma de bar aberto” atrai 17 projectos, Start Campus concorre com dois","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1860265?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"},{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tip
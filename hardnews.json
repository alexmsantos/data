uthor":[{"id":7026,"nome":"Tiago Ramalho","descricao":"","email":"tiago.ramalho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"tiago-ramalho","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"tiagoramalho.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1328997?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#97;&#109;&#97;&#114;&#46;&#111;&#103;&#97;&#105;&#116;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/09/28/ciencia/noticia/primeira-provas-buracos-negros-giram-2064887","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Astrofísica","url":"https://www.publico.pt/2023/09/28/ciencia/noticia/primeira-provas-buracos-negros-giram-2064887"},"id":2064887,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Seis jovens a lutar pela acção climática no TEDH: “Nada disto é política. É ciência”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1845665?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6488,"nome":"Aline Flor","descricao":"Natural do Rio de Janeiro, vivo em Portugal há mais de 25 anos. Criei raízes. Fui directora do Jornal Universitário do Porto, passei pela Ciência e Tecnologia no Canal Superior, editei notícias do portal AEIOU.pt. Fiz um mestrado em Cinema Documental que me abriu os olhos (e os ouvidos) para a riqueza de cada encontro com o outro. Cheguei ao PÚBLICO para integrar a equipa de áudio, onde produzi podcasts em áreas desde a igualdade de género às questões europeias. Hoje faço parte do Azul, a nossa secção dedicada à crise climática e sustentabilidade. Gosto de ouvir e de contar as histórias que ficam à margem e acredito que temos sempre algo a aprender quando encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"Estrasburgo","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/09/27/azul/noticia/seis-jovens-lutar-accao-climatica-estrasburgo-nada-politica-ciencia-2064832","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Tribunal Europeu dos Direitos Humanos","url":"https://www.publico.pt/2023/09/27/azul/noticia/seis-jovens-lutar-accao-climatica-estrasburgo-nada-politica-ciencia-2064832"},"id":2064832,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Barril de petróleo cada vez mais perto dos 100 dólares","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1845756?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1022,"nome":"Victor Ferreira","descricao":"Teria sido médico, se desse mais ouvidos aos meus pais, ou matemático ou bioquímico, se me tivesse aplicado a concluir os primeiros cursos superiores em que me matriculei. Porém, no Verão dos meus 18 anos, entrei pela primeira vez numa redacção. Era um jornal local, Guimarães, onde ainda hoje vive a minha família. Essa curta experiência, enquanto esperava pela universidade, mudou as minhas aspirações. Anos depois, pendurei um hipotético futuro de bata
l/). A identidade está em movimento.","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":", em Jenin (Cisjordânia)","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"alexandra-lucas-coelho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Alexandra Lucas Coelho NOVA.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1850498?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Prémio Oceanos 2022","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/23/mundo/reportagem/debaixo-drone-jenin-hospital-casa-comandante-2074662","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/medio-oriente"},"id":2074662,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":4,"title":"Rendas nas grandes cidades já são 35% superiores às do resto do país","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1869455?tp=UH&db=IMAGENS&type=JPG","author":[{"id":15415,"nome":"Rafaela Burd Relvas","descricao":"","email":"rafaela.relvas@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"rafaela-burd-relvas","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#118;&#108;&#101;&#114;&#46;&#97;&#108;&#101;&#97;&#102;&#97;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/23/economia/noticia/rendas-cidades-ja-sao-35-superiores-resto-pais-2074648","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Habitação","url":"https://www.publico.pt/2023/12/23/economia/noticia/rendas-cidades-ja-sao-35-superiores-resto-pais-2074648"},"id"
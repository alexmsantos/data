\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":197,"url":"/2024/05/04/politica/noticia/pedro-nuno-pede-ajuda-js-reconquistar-eleitorado-jovem-combater-extremadireita-2089246","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PS","url":"https://www.publico.pt/2024/05/04/politica/noticia/pedro-nuno-pede-ajuda-js-reconquistar-eleitorado-jovem-combater-extremadireita-2089246"},"id":2089246,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":5,"title":"Marcelo: de “absolutamente consensual” a uma queda de popularidade irrecuperável","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1905554?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3898,"nome":"Liliana Borges","descricao":"","email":"liliana.borges@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"liliana-pascoal-borges","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"liliana-borges.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1172095?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#
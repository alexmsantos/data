este curso, que então se chamava Comunicação Social, no ano lectivo de 1979/80.","email":"cviana@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"clara-viana","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#97;&#105;&#118;&#99;","contribuicao":"","tipo":"INTERNO"},{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":1,"url":"/2023/10/30/sociedade/noticia/ministerio-aceita-maior-rigor-formacao-professores-sindicatos-pedem-novas-negociacoes-2068492","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Ministério da Educação","url":"https://www.publico.pt/2023/10/30/sociedade/noticia/ministerio-aceita-maior-rigor-formacao-professores-sindicatos-pedem-novas-negociacoes-2068492"},"id":2068492,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":9,"title":"Acelerar a descarbonização no sector da saúde","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1853421?tp=UH&db=IMAGENS&type=PNG","author":[],"shares":0,"url":"/2023/10/25/estudiop/noticia/acelerar-descarbonizacao-sector-saude-2067946","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Astrazeneca","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1303880?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2023/10/25/estudiop/noticia/acelerar-descarbonizacao-sector-saude-2067946"},"id":2067946,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Rendas sem travão. O q
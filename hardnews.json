anhei três menções honrosas no concurso de fotojornalismo da revista Visão e venci a categoria de fotografia de arquitectura do Euro Press Photo Awards da Fuji Film. Fui premiado no Prémio Estação Imagem|Mora em 2010 e 2011. Desde 1999, sou fotojornalista no PÚBLICO.","email":"ngarrido@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"nelson-garrido","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Nelson_Garrido.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737874?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#105;&#114;&#114;&#97;&#103;&#110;","contribuicao":"","tipo":"INTERNO"}],"shares":35,"url":"/2025/01/12/local/reportagem/ultima-viagem-comboio-coimbra-menos-encanto-hora-despedida-2118516","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/local"},"id":2118516,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":23,"title":"Rui Pinto volta a tribunal. Do Benfica à segurança, saiba tudo sobre este julgamento","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1969504?tp=UH&db=IMAGENS&type=JPG","author":[{"id":7165,"nome":"Miguel Dantas","descricao":"","email":"miguel.dantas@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"miguel-dantas","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#116;&#110;&#97;&#100;&#46;&#108;&#101;&#117;&#103;&#105;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":30,"url":"/2025/01/12/sociedade/not
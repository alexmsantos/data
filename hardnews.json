bre pais que perderam os seus filhos. Fui editora do P2, editora da Sociedade, editora-executiva. Actualmente sou directora-adjunta.","email":"asanches@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Directora-adjunta","profissaoNaAltura":"","slug":"andreia-sanches","imagem":{"w":225,"h":225,"largura":225,"altura":225,"nome":"andreia-sanches-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1748990?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":"Andreia C. Faria","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#104;&#99;&#110;&#97;&#115;&#97;","contribuicao":"","tipo":"INTERNO"},{"id":6039,"nome":"Cristiana Faria Moreira","descricao":"","email":"cristiana.moreira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista","profissaoNaAltura":null,"slug":"cristiana-faria-moreira","imagem":{"w":225,"h":225,"largura":225,"altura":225,"nome":"Cristiana Faria Moreira.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1602146?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#111;&#109;&#46;&#97;&#110;&#97;&#105;&#116;&#115;&#105;&#114;&#99;","contribuicao":null,"tipo":"INTERNO"}],"shares":0,"url":"/2024/12/19/newsletter/educacao","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Educação","url":""},"id":2116279,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":24,"title":"“Muito sigilo” e pouca cooperação: as reprimendas de Marcelo a Montenegro","isPremium":true,"image":"https://imagens.publico.pt/
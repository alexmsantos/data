pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Copydesk, Jornalista","profissaoNaAltura":"Copydesk, Jornalista","slug":"rita-pimenta","imagem":{"w":null,"h":null,"largura":422,"altura":423,"nome":"EuCarteiraProfissional.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1647987?tp=UH&db=IMAGENS&type=JPG","is_vertical":true,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#110;&#101;&#109;&#105;&#112;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/23/impar/noticia/nao-ha-chamine-consegue-pai-natal-entrar-2074634","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"impar"},"brand":null,"rubric":{"name":"Livros","url":""},"id":2074634,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":26,"title":"<em>Last Christmas</em> dos Wham! chega a número um no Natal 39 anos após lançamento\r\n","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1869617?tp=UH&db=IMAGENS&type=PNG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2023/12/23/culturaipsilon/noticia/last-christmas-wham-chega-numero-natal-39-anos-apos-lancamento-2074705","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Música","url":"https://www.publico.pt/2023/12/23/culturaipsilon/noticia/last-christmas-wham-chega-numero-natal-39-anos-apos-lancamento-2074705"},"id":2074705,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":27,"title":"Fim-de-semana e Natal na TV: Gervais, Gisela, Jimmy
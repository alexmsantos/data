odeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":225,"h":225,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"},{"id":890,"nome":"Reuters","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"reuters","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"AGENCIAS"}],"shares":0,"url":"/2024/06/17/azul/noticia/governos-aprovam-lei-restauro-natureza-finalmente-entrar-vigor-2094227","comments":2,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"União Europeia","url":"https://www.publico.pt/2024/06/17/azul/noticia/governos-aprovam-lei-restauro-natureza-finalmente-entrar-vigor-2094227"},"id":2094227,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Quase uma década depois, nenhum dos ODS está a caminho de ser alcançado","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1917077?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6488,"nome":"Aline Flor","descricao":"Natural do Rio de Janeiro, vivo em Portugal há mais de 25 anos. Criei raízes. Fui directora do Jornal Universitário do Porto, passei pela Ciência e Tecnologia no Canal Superior, editei notícias do portal AEIOU.pt. Fiz um mestrado em Cinema Documental que me abriu os olhos (e os ouvidos) para a riqueza de cada encontro com o outro. Cheguei ao
 sem os filmes não saberia o que fazer.","email":"vasco.camara@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"em Veneza","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"vasco-camara","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"vasco-camara-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1747423?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Cannes","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#97;&#109;&#97;&#99;&#46;&#111;&#99;&#115;&#97;&#118;","contribuicao":"","tipo":"INTERNO"}],"shares":13,"url":"/2025/09/03/culturaipsilon/noticia/voz-hind-rajab-ecoe-mundo-veneza-fezse-ouvir-2145861","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Cinema","url":"https://www.publico.pt/2025/09/03/culturaipsilon/noticia/voz-hind-rajab-ecoe-mundo-veneza-fezse-ouvir-2145861"},"id":2145861,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Casa aberta para celebrar o Vinho do Porto","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2025197?tp=UH&db=IMAGENS&type=PNG","author":[],"shares":0,"url":"/2025/09/02/estudiop/noticia/casa-aberta-celebrar-vinho-porto-2144901","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Vinho do Porto","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1760096?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2025/09/02/estudiop/noticia/casa-aberta-celebrar-vinho-porto-2144901"},"id":2144901,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"David Byrne canta o prazer das pequenas histórias","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2026977?t
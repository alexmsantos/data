 meu) quase nunca se repete.","email":"sonia.sapage@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Directora-adjunta","profissaoNaAltura":"","slug":"sonia-sapage","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"sonia-sapage-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739859?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#101;&#103;&#97;&#112;&#97;&#115;&#46;&#97;&#105;&#110;&#111;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/10/22/sociedade/entrevista/joao-neto-cartelizacao-mercado-trabalho-favoreceu-exploracao-medicos-dentistas-2067553","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/servico-nacional-de-saude"},"id":2067553,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Portugal aposta no hidrogénio para acelerar a transição energética","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1851021?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":19,"url":"/2023/10/18/estudiop/noticia/portugal-aposta-hidrogenio-acelerar-transicao-energetica-2066936","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"HIDROGÉNIO","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1841089?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2023/10/18/estudiop/noticia/portugal-aposta-hidrogenio-acelerar-transicao-energetica-2066936"},"id":2066936,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":14,"title":"Afinal, o que quer o Hamas?","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1852585?tp=UH&db=IMAGENS&type=JPG","author":[{"
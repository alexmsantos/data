nda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1527215?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":",Norges teknisk-naturvitenskaplige universitet","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#109;&#105;&#108;&#46;&#111;&#105;&#110;&#111;&#116;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/01/12/mundo/noticia/israel-rejeita-acusacoes-genocidio-gaza-tribunal-internacional-justica-2076600","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Médio Oriente","url":"https://www.publico.pt/2024/01/12/mundo/noticia/israel-rejeita-acusacoes-genocidio-gaza-tribunal-internacional-justica-2076600"},"id":2076600,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":9,"title":"Houthis dizem que ataques no Iémen \"não ficarão impunes ou sem resposta\"","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1874247?tp=UH&db=IMAGENS&type=JPG","author":[{"id":890,"nome":"Reuters","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"","profissaoNaAltura":"","slug":"reuters","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":null,"tipo":"AGENCIAS"},{"id":3925,"nome":"Joana Bourgard","descricao":"Sou jornalista desde 2007 e, ao longo destes anos de profissão, é a terceira vez que estou no PÚBLICO. Comecei como estagiária, na secção de Fotografia do jornal. Prossegui fotojornalismo no Jornal de Notícias, no Porto, onde fiquei três anos. Foi nessa altura que apareceram os smartphones e as primeiras máquinas fotográficas digitais com a possibilidade de captar imagem em movimento. Tornei-me jornalista multimédia e regressei para Lisboa para fazer parte da equipa de vídeo do PÚBLICO, em 2010. Passado quatro anos juntei-me à equipa multimédia da Rádio Renascença. Por fim, 15 anos depois do estágio no PÚBLICO, regressei em 2022 para editar a secção de Multimédia. Em 2007 fiz parte fundação da <a href=\"https://wilder.pt\">Wilder.pt</a>, um projecto de jornalismo independente dedicado à natureza e biodiversidade.","email":"joana.bourgard@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Editora de Multimédia","profissaoNaAltura":"Editora de Multimédia","slug":"joana-bourgard","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Joana_Bourgard.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737812?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#100;&#114;&#97;&#103;&#114;&#117;&#111;&#98;&#46;&#97;&#110;&#97;&#111;&#106;","contribuicao":null,"tipo":"INTERNO"}],"shares":0,"url":"/2024/01/12/mundo/video/houthis-ataques-iemen-nao-ficarao-impunes-resposta-20240112-115450","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Vídeo","url":"/video-"},"id":95894,"type":"VIDEO","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Homem armado assalta banco na Madeira e foge com mais de 240 mil euros","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1874246?tp=UH&db=IMAGENS&type=JPG","author":[{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2024/01/12/sociedade/noticia/homem-armado-assalta-banco-madeira-foge-240-mil-euros-2076607","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,
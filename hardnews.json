 anos, a crise financeira internacional e a crise da dívida soberana europeia têm ocupado grande parte do meu tempo e atenção.","email":"sergio.anibal@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sergio-anibal","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Anibal.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737905?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#97;&#98;&#105;&#110;&#97;&#46;&#111;&#105;&#103;&#114;&#101;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/11/26/economia/noticia/orcamento-estado-nao-cumpre-integralmente-recomendacoes-avisa-bruxelas-2113390","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Finanças públicas","url":"https://www.publico.pt/2024/11/26/economia/noticia/orcamento-estado-nao-cumpre-integralmente-recomendacoes-avisa-bruxelas-2113390"},"id":2113390,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":7,"title":"Valor mediano da avaliação das casas já está acima de 1700 euros","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1958252?tp=UH&db=IMAGENS&type=JPG","author":[{"id":459,"nome":"Rosa Soares","descricao":"","email":"rsoares@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"rosa-soares","imagem":{"w":null,"h":null,"largura":472,"altura":472,"nome":"rosa soares.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1390182?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true
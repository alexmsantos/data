alidades. O desporto dos outros é a minha profissão.","email":"mvaza@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"marco-vaza","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Marco_Vaza.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737861?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#122;&#97;&#118;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":34,"url":"/2023/12/06/culturaipsilon/noticia/vamos-abracar-reabracar-amigos-gaspar-2072536","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Cinema","url":"https://www.publico.pt/2023/12/06/culturaipsilon/noticia/vamos-abracar-reabracar-amigos-gaspar-2072536"},"id":2072536,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Vinho do Porto: a certeza de ocasiões felizes","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1864554?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2023/12/05/estudiop/noticia/vinho-porto-certeza-ocasioes-felizes-2072088","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Vinho do Porto","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1760096?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2023/12/05/estudiop/noticia/vinho-porto-certeza-ocasioes-felizes-2072088"},"id":2072088,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Na Grécia, moradores fingem pragas de percevejos para afastar turistas dos AL","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1864922?tp=UH&db=IMAGENS&type=JPG","author":[{"i
teratura: Ciberescritas. Fiz parte da equipa da revista Pública. Fui editora do suplemento literário Mil Folhas, de Fevereiro de 2002 a Fevereiro de 2007. E também fui editora da Cultura.","email":"isabel.coutinho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"isabel-coutinho","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Isabel_Coutinho.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/738284?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#110;&#105;&#116;&#117;&#111;&#99;&#46;&#108;&#101;&#98;&#97;&#115;&#105;","contribuicao":"","tipo":"INTERNO"},{"id":4716,"nome":"José Riço Direitinho","descricao":null,"email":null,"facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":null,"profissaoNaAltura":"","slug":"jose-rico-direitinho","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2025/08/28/culturaipsilon/noticia/hannah-arendt-tempos-trump-novo-romance-jacinto-lucas-pires-2145184","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Saída de livros","url":"https://www.publico.pt/2025/08/28/culturaipsilon/noticia/hannah-arendt-tempos-trump-novo-romance-jacinto-lucas-pires-2145184"},"id":2145184,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":19,"title":"Como o mapa de um rumor durante a Revolução Francesa veio pôr fim a debate histórico","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2025705?tp=UH&db=IMAGENS&type=JPG","author":[{"id":7026,"nome":"Tiago Ramalho","descricao":"","email":"tiago.ramalho@publico.pt","facebook"
anceira internacional e a crise da dívida soberana europeia têm ocupado grande parte do meu tempo e atenção.","email":"sergio.anibal@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sergio-anibal","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Anibal.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737905?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#97;&#98;&#105;&#110;&#97;&#46;&#111;&#105;&#103;&#114;&#101;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/29/economia/noticia/ocde-baixa-previsao-crescimento-portugues-12-2024-2071932","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PIB","url":"https://www.publico.pt/2023/11/29/economia/noticia/ocde-baixa-previsao-crescimento-portugues-12-2024-2071932"},"id":2071932,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Desafios e inspirações na construção de um futuro sustentável","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1861784?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":10,"url":"/2023/11/24/estudiop/noticia/desafios-inspiracoes-construcao-futuro-sustentavel-2071079","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"UCP Porto","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1857888?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2023/11/24/estudiop/noticia/desafios-inspiracoes-construcao-futuro-sustentavel-2071079"},"id":2071079,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Investigações fiscais com troc
aço parte do Azul, a nossa secção dedicada à crise climática e sustentabilidade. Gosto de ouvir e de contar as histórias que ficam à margem e acredito que temos sempre algo a aprender quando encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"},{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/09/13/azul/noticia/verao-nao-acabou-fimdesemana-noites-tropicais-temperaturas-30c-2104059","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Meteorologia","url":"https://www.publico.pt/2024/09/13/azul/noticia/verao-nao-acabou-fimdesemana-noites-tropicais-temperaturas-30c-2104059"},"id":2104059,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Ex-director das prisões aponta falhas de segurança aos guardas de Vale de Judeus","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1938273?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":nu
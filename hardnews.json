k":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"rui-gaudencio","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#105;&#99;&#110;&#101;&#100;&#117;&#97;&#103;&#114;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":0,"url":"/2025/09/16/ciencia/entrevista/envelhecimento-ate-ninguem-conseguiu-rejuvenescer-ninguem-2147292","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/longevidade"},"id":2147292,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Boas notícias: a este ritmo, o buraco da camada de ozono pode recuperar em 2040","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2029817?tp=UH&db=IMAGENS&type=JPG","author":[{"id":204,"nome":"Andrea Cunha Freitas","descricao":"Tenho uma licenciatura em Jornalismo Internacional e sou jornalista no PÚBLICO desde 1997. Trabalhei na secção do Local e da Sociedade, onde fui editora. Desde 2016 sou jornalista na secção de Ciência.","email":"acfreitas@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"andrea-cunha-freitas","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#116;&#105;&#101;&#114;&#102;&#99;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":17,"url":"/2025/09/16/azul/noticia/boas-noticias-buraco-camada-ozono-recuperar-2040-tarde-polos-2147208","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Alterações climáticas","url":"https://www.publico.pt/2025/09/16/azul/noticia/boas-noticias-buraco-camada-ozono-recuperar-2040-tarde-polos-2147208"},"id":2147208,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Entra em vigor acordo contra subsídios prejudiciais à pesca. Mas isto é só o início","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2029971?tp=UH&db=IMAGENS&type=JPG","author":[{"id":227,"nome":"Clara Barata","descricao":"Gosto de compreender como funciona o mundo. Numa célula, de me espantar como uma máquina biológica esconde a centelha que a transforma numa coisa viva. Nas manobras de bastidores de uma campanha de um político que quer ser eleito Presidente da República, descobre-se um homem e uma teia de interesses tão mais vasta que ele e muito mais que isso: as aspirações, os traumas, os preconceitos e a cultura de um povo que pode ser muito diferente do nosso. Os meus interesses são variados, da ciência à política internacional, ambiente ou tendências sociais, na Europa, nos EUA. Um ponto comum entre eles é sempre a vontade de querer saber como funcionam as coisas — e aplicar uma atitude que aprendi a assumir fazendo jornalismo de ciência, que é não ter medo de fazer perguntas, mesmo que pareçam básicas.","email":"clara.barata@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"clara-barata","imagem":{"w":null,"h":null,"largura":368,"altura":331,"nome":"_RG__foto_Clara_Barata_DSCF4152-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1926383?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#97;&#114;&#97;&#98;&#46;&#97;&#114;&#97;&#108;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/09/16/azul/noticia/entra-vigor-acordo-subsidios-prejudiciais-pesca-so-inicio-2147218","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Pesca","url":"https://www.publico.pt/2025/09/16/azul/noticia/entra-vigor-acordo-subsidios-prejudiciais-pesca-so-inicio-2147218"},"id":2147218,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"<em>O Agente Secreto</em>, primeira batalha dos fãs: é o filme que o Brasil envia ao Óscar","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2029898?tp=UH&db=IMAGENS&type=JPG","author":[{"id":885,"nome":"Vasco Câmara","descricao":"Foi depois de ter entrado pela casa que Gloria Swanson teve em Sintra, onde vi junto à piscina um animal empalhado, que me deixaram, aqui no PÚBLICO, meter as mãos no cinema. Isto foi no início deste projecto jornalístico. Eu fui um dos 25 estagiários admitidos depois de um curso de meses que começou com 500 candidatos de todo o país. Após a história da estrela dos anos 20 que vinha descansar perto da praia das Maçãs, alguém me disse: “Faça as malas, amanhã vai para Cannes”. E foi, no ano do fósforo a arder no ecrã gigante do Palais des Festivals, o ano de “Wild at Heart” de Lynch. Se não fossem os filmes não saberia o que estaria a fazer aqui – e isso é verdade para o bigger picture: sem os filmes não saberia o que fazer.","email":"vasco.camara@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"vasco-camara","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"vasco-camara-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1747423?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Cannes","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#97;&#109;&#97;&#99;&#46;&#111;&#99;&#115;&#97;&#118;","contribuicao":"","tipo":"INTERNO"}],"shares":25,"url":"/2025/09/15/culturaipsilon/noticia/agente-secreto-primeira-batalha-fas-filme-brasil-envia-oscar-2147275","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Cinema","url":"https://www.publico.pt/2025/09/15/culturaipsilon/noticia/agente-secreto-primeira-batalha-fas-filme-brasil-envia-oscar-2147275"},"id":2147275,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":19,"title":"Mondo Duplantis, a “criança” que não deixa mais ninguém brincar","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2029942?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6063,"nome":"Diogo Cardoso Oliveira","descricao":"Ver desporto, escrever e contar histórias: costumo dizer que sou pago para fazer algo que faria <i>pro-bono</i>. Tudo começou numa t-shirt branca, feita na escola primária, que já identificava um gaiato com más ideias: dizia “Diogo, o jornalista”. Feito um raide incompleto pela aviação, formei-me em Comunicação Social, época em que fazia do jornal universitário <i>Pontivirgula</i> uma tela para rascunhos do que estaria por vir. Comecei a carreira como estagiário no PÚBLICO, no mês em que Éder decidiu dar um chuto aos franceses. Ainda estive no <i>Record</i>, antes de fazer parte do início do site <i>Bancada</i>. Voltei para o PÚBLICO, que já me levou ao Mundial do Qatar, aos Jogos Olímpicos de Paris e onde faço um pouco de tudo no desporto. Gosto de ciclismo, basquetebol, ginástica, futebol, História e histórias — também por isso escrevi o livro <i>Futebol numa conversa de café</i>.","email":"diogo.oliveira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"diogo-cardoso-oliveira","imagem":{"w":null,"h":null,"largura":1064,"altura":1345,"nome":"WhatsApp Image 2025-04-24 at 20.31.38.jpeg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1995019?tp=UH&db=IMAGENS&type=JPG","is_vertical":true,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#118;&#105;&#108;&#111;&#46;&#111;&#103;&#111;&#105;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/09/15/desporto/noticia/mondo-duplantis-crianca-nao-deixa-ninguem-brincar-2147300","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Atletismo","url":"https://www.publico.pt/2025/09/15/desporto/noticia/mondo-duplantis-crianca-nao-deixa-ninguem-brincar-2147300"},"id":2147300,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":20,"title":"Estado paga subvenção vitalícia a 242 ex-políticos e juízes do TC","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2029956?tp=UH&db=IMAGENS&type=JPG","author":[{"id":157,"nome":"Maria Lopes","descricao":"Entrei para o PÚBLICO, vinda da universidade, quando se vivia a excitação das vésperas da primeira Exposição Mundial em Portugal. Calcorreei a Expo-98 de fio a pavio, de caderno, caneta e, por vezes, de Polaroid na mão. Esses poucos meses ensinaram-me a conjugar o verbo tão português “desenrascar-se”.\r\nNestes mais de 20 anos, passei depois pelas secções Local, Media (por duas vezes) e Economia. Estive no lançamento do suplemento Digital – que apesar do nome existiu apenas em papel – e pertenci à equipa do irreverente projecto Sexta, o jornal gratuito que o PÚBLICO partilhou com A Bola, em 2007 e 2008. Em simultâneo, durante dez anos, escrevi também para o Motores (da Fugas), vivendo os automóveis sob uma perspectiva menos técnica e mais prazenteira. Desde 2009 que integro a secção de Política e tento manter debaixo de olho o panorama dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/09/15/politica/noticia/estado-paga-subvencao-vitalicia-242-expoliticos-juizes-contagem-tempo-acabou-ha-20-anos-2147308","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Transparência","url":"https://www.publico.pt/2025/09/15/politica/noticia/estado-paga-subvencao-vitalicia-242-expoliticos-juizes-contagem-tempo-acabou-ha-20-anos-2147308"},"id":2147308,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":21,"title":"Faltam professores, mas directores admitem situação “menos complicada”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2029955?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6039,"nome":"Cristiana Faria Moreira","descricao":"","email":"cristiana.moreira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"cristiana-faria-moreira","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Cristiana Faria Moreira.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1602146?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#111;&#109;&#46;&#97;&#110;&#97;&#105;&#116;&#115;&#105;&#114;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/09/15/sociedade/noticia/faltam-professor
uthor":[{"id":749,"nome":"Helena Pereira","descricao":"Comecei a ser jornalista em 1996, no PÚBLICO, na editoria de Política, depois de um estágio na TSF. Em 2006, participei na fundação do semanário SOL, como jornalista de Política e, mais tarde, na fundação do jornal Observador, como editora de Política. Em 2016, passei para o Expresso também como editora de Política até ser convidada a regressar ao PÚBLICO em 2018 para ser editora executiva. Nasci num belo ano, 1974, na Maternidade Alfredo da Costa, em Lisboa, e sou mãe de gémeos, o Jaime e a Júlia.","email":"helena.pereira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editora executiva","profissaoNaAltura":"","slug":"helena-pereira","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"helena-pereira-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739855?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Público","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#101;&#112;&#46;&#97;&#110;&#101;&#108;&#101;&#104;","contribuicao":"","tipo":"INTERNO"},{"id":7253,"nome":"Susana Madureira Martins (Renascença)","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Renascença","profissaoNaAltura":"","slug":"susana-madureira-martins","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"},{"id":3920,"nome":"Daniel Rocha","descricao":"","email":"drocha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"daniel-rocha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Daniel_Rocha.jpg","legenda":null,"autor":null,"tipo":"
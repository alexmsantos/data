ra@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":", em Cannes","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"vasco-camara","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"vasco-camara-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1747423?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Cannes","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#97;&#109;&#97;&#99;&#46;&#111;&#99;&#115;&#97;&#118;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/05/17/culturaipsilon/noticia/fotografa-palestiniana-fatma-hassona-morreu-aqui-2133420","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Festival de Cannes","url":"https://www.publico.pt/2025/05/17/culturaipsilon/noticia/fotografa-palestiniana-fatma-hassona-morreu-aqui-2133420"},"id":2133420,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":9,"title":"A transição energética precisa de dados abertos","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1999800?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2025/05/14/estudiop/noticia/transicao-energetica-precisa-dados-abertos-2132873","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"E-REDES","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1584526?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2025/05/14/estudiop/noticia/transicao-energetica-precisa-dados-abertos-2132873"},"id":2132873,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Exercício militar destruiu vida a fuzileiro. Estado foi condenado, mas recorreu","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1
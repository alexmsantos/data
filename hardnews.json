ias sociais, na Europa, nos EUA. Um ponto comum entre eles é sempre a vontade de querer saber como funcionam as coisas — e aplicar uma atitude que aprendi a assumir fazendo jornalismo de ciência, que é não ter medo de fazer perguntas, mesmo que pareçam básicas.","email":"clara.barata@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"clara-barata","imagem":{"w":null,"h":null,"largura":368,"altura":331,"nome":"_RG__foto_Clara_Barata_DSCF4152-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1926383?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#97;&#114;&#97;&#98;&#46;&#97;&#114;&#97;&#108;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/11/azul/noticia/alteracoes-climaticas-tornaram-duas-vezes-provaveis-furacoes-milton-2107581","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Furacões","url":"https://www.publico.pt/2024/10/11/azul/noticia/alteracoes-climaticas-tornaram-duas-vezes-provaveis-furacoes-milton-2107581"},"id":2107581,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":23,"title":"Encontrado pé de alpinista desaparecido há 100 anos no Evereste","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1945815?tp=UH&db=IMAGENS&type=JPG","author":[{"id":18692,"nome":"Ana Torres","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"ana-torres","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/11/mundo/noticia/encontrado-pe-alpinista-desaparecido
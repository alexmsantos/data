luções. Nunca mais parei de fazer do outro mundo o meu. E até aprendi a escrever Saddam em árabe.","email":"slorena@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sofia-lorena","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"sofia-lorena-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739860?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#101;&#114;&#111;&#108;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":23,"url":"/2023/10/26/mundo/noticia/gaza-jornalistas-sao-mortos-cobrir-conflito-dentro-casa-2068085","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Médio Oriente","url":"https://www.publico.pt/2023/10/26/mundo/noticia/gaza-jornalistas-sao-mortos-cobrir-conflito-dentro-casa-2068085"},"id":2068085,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":24,"title":"Eles fotografam Gaza e mostram o que está a acontecer no Instagram","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1853799?tp=UH&db=IMAGENS&type=JPG","author":[{"id":17576,"nome":"Margarida Leite Gonçalves","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"margarida-leite-goncalves","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":3,"url":"/2023/10/26/p3/noticia/fotografam-gaza-mostram-acontecer-instagram-2068088","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"p3"},"brand":null,"rubric":{"name":"Médio Oriente","url":"https://www.publico.pt/2023/10/26/p3/noticia/fotografam-gaza-mos
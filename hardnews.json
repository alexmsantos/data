:[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/07/19/azul/noticia/caravelasportuguesas-alforrecas-podem-estar-aumentar-sabado-dia-contar-2098160","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Praias","url":"https://www.publico.pt/2024/07/19/azul/noticia/caravelasportuguesas-alforrecas-podem-estar-aumentar-sabado-dia-contar-2098160"},"id":2098160,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":27,"title":"A temperatura da água do mar pode utrapassar os 26 graus nos Açores","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1925885?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/07/20/azul/noticia/ipma-preve-temperatura-agua-mar-ultrapasse-26-graus-acores-2098292","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Açores","url":"https://www.publico.pt/2024/07/20/azul/noticia/ipma-preve-temperatura-agua-mar-ultrapasse-26-graus-acores-2098292"},"id":2098292,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":28,"title":"Com pouca margem para cedências, Governo consegue segurar PS e Chega até Setembro","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1925789?tp=UH&db=IMAGENS&type=JPG","author":[{"id":157,"nome":"Maria Lopes","descricao":"Entrei para o PÚBLICO, vinda da universidade, quando se vivia a excitação das vésperas da primeira Exposição Mundial em Portugal. Calcorreei a Expo-98 de fio a pavio, de caderno, caneta e, por vezes, de Polaroid na mão. Esses poucos meses ensinaram-me a conjugar o verbo tão português “desenrascar-se”.\r\nNestes mais de 20 anos, passei depois pelas secções Local, Media (por duas vezes) e Economia. Estive no lançamento do suplemento Digital – que apesar do nome existiu apenas em papel – e pertenci à equipa do irreverente projecto Sexta, o jornal gratuito que o PÚBLICO partilhou com A Bola, em 2007 e 2008. Em simultâneo, durante dez anos, escrevi também para o Motores (da Fugas), vivendo os automóveis sob uma perspectiva menos técnica e mais prazenteira. Desde 2009 que integro a secção de Política e tento manter debaixo de olho o panorama dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/07/19/politica/noticia/margem-cedencias-governo-consegue-segurar-ps-chega-ate-setembro-2098262","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Orçamento do Estado 2025","url":"https://www.publico.pt/2024/07/19/politica/noticia/margem-cedencias-governo-consegue-segurar-ps-chega-ate-setembro-2098262"},"id":2098262,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Na Roederer, o champanhe obedece às leis da relojoaria","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1925331?tp=UH&db=IMAGENS&type=JPG","author":[{"id":953,"nome":"Manuel Carvalho","descricao":"Nasci em Alijó, Alto Douro, em 1965, estudei na extinta Escola Normal do Porto, fui professor durante dois anos e fiz parte do grupo de estagiários que integraram a primeira redacção do PÚBLICO, no final de 1989. Durante quase uma década trabalhei na área da Economia, tendo recebido vários prémios de jornalismo desta área. Neste período fiz estágios e programas de formação em Portugal, Japão (International Studies and Training), Estados Unidos (German Marshall Fund) e Bélgica e escrevi, em autoria exclusiva ou em co-autoria, os meus primeiros livros sobre uma das minhas paixões pessoais: o Douro e a memória do vinho do Porto. Fui grande repórter do Diário Económico e regressei ao PÚBLICO em 1999. Aí, fui grande repórter, lancei a Fugas em parceria com o nosso ex-colega David Lopes Ramos e entrei na direcção do jornal em 2000. Entretanto, estudei Direito na Faculdade de Direito da Universidade do Porto e História na Faculdade de Letras, onde me licenciei. Continuei a escrever sobre vinho (a enofilia é endémica nos durienses), fiz as eleições do Brasil desde o primeiro triunfo de Lula, em 2002. Em 2013 saí da direcção do Público e, como Redactor Principal, estreei a “Memória Futura”, a minha coluna de opinião semanal. E continuei a escrever sobre tudo um pouco, principalmente nas áreas da economia associadas ao desenvolvimento regional, à indústria e à agricultura. Pelo meio, recebi o Prémio Gazeta de Imprensa, em 2014, por uma série de reportagens sobre a I Guerra Mundial em Moçambique que fiz na companhia do fotojornalista Manuel Roberto. Essas reportagens dariam mais tarde lugar à edição do livro “A Guerra que Portugal Quis Esquecer” (Ed. Porto Editora). Em Julho de 2018 assumi o desafio de dirigir a mais completa, diversificada e talentosa redacção do país: a do meu jornal de sempre.","email":"manuel.carvalho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactor principal","profissaoNaAltura":"","slug":"manuel-carvalho","imagem":{"w":null,"h":null,"largura":573,"altura":382,"nome":"Manuel Carvalho recentrado.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1901068?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":"Pedro Passos Coelho","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#97;&#118;&#114;&#97;&#99;&#46;&#108;&#101;&#117;&#110;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/07/20/fugas/reportagem/roederer-champanhe-obedece-leis-relojoaria-2097617","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"fugas"},"brand":null,"rubric":{"name":"Reportagem","url":"/franca"},"id":2097617,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

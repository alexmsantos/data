ares":13,"url":"/2024/05/26/azul/reportagem/celeiro-grecia-tenta-erguerse-lama-2091181","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/catastrofes-naturais"},"id":2091181,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":21,"title":"Próximo destino: aventura","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1910057?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2024/05/24/estudiop/noticia/proximo-destino-aventura-2091269","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"FORD","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1910011?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2024/05/24/estudiop/noticia/proximo-destino-aventura-2091269"},"id":2091269,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":22,"title":"“Sou e serei uma alcoólica em recuperação”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1911592?tp=UH&db=IMAGENS&type=JPG","author":[{"id":469,"nome":"Joana Amado","descricao":"Entrei para o PÚBLICO três anos depois da sua fundação, vinda de um curso de Comunicação da Universidade Nova de Lisboa. Foi no PÚBLICO que tive o privilégio de aprender a escrever para transmitir aos outros conhecimento, informação e emoções. Contar histórias com cores, cheiros, sabores e ritmos, denunciar os horrores da guerra, da miséria e da injustiça, partilhar prazeres e descobertas, desvendando todos os dias um pouco mais do mundo. Fui dos campos de refugiados kosovares na Albânia aos vibrantes países da Nova Europa, da Florida que deu a vitória a Bush à Las Vegas do póquer e dos imitadores do Elvis, da reeleição de Tony Blair à Chueca gay, em Madrid. Hoje comando uma equipa de jornalistas a quem peço isso mesmo todos os dias: dêem-me mais mundo, a mim e a todos os que lêem o PÚBLICO.","email":"jamado@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"joana-amado","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Joana_Amado.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737811?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":true,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#97;&#109;&#97;&#106;","contribuicao":"","tipo":"EXTERNO"}],"shares":18,"url":"/2024/05/26/sociedade/noticia/problema-problema-historia-adiccao-2091414","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Testemunho","url":""},"id":2091414,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":23,"title":"Morreu Darryl Hickman, que nasceu como actor aos nove anos com John Ford","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1911765?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11132,"nome":"Brian Murphy","descricao":"Jornalista The Washington Post","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista The Washington Post","profissaoNaAltura":"","slug":"brian-murphy","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"PROJECT_SYNDICATE"}],"shares":0,"url":"/2024/05/26/culturaipsilon/noticia/morreu-darryl-hickman-nasceu-actor-nove-anos-john-ford-2091854","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Cinema","url":"https://www.publico.pt/2024/05/26/culturaipsilon/noticia/morreu-darryl-hickman-nasceu-actor-nove-anos-john-ford-2091854"},"id":2091854,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":24,"title":"Paul Parkman (1932-2024), o cientista que inventou a vacina da rubéola","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1911317?tp=UH&db=IMAGENS&type=JPG","author":[{"id":227,"nome":"Clara Barata","descricao":"Gosto de compreender como funciona o mundo. Numa célula, de me espantar como uma máquina biológica esconde a centelha que a transforma numa coisa viva. Nas manobras de bastidores de uma campanha de um político que quer ser eleito Presidente da República, descobre-se um homem e uma teia de interesses tão mais vasta que ele e muito mais que isso: as aspirações, os traumas, os preconceitos e a cultura de um povo que pode ser muito diferente do nosso. Os meus interesses são variados, da ciência à política internacional, ambiente ou tendências sociais, na Europa, nos EUA. Um ponto comum entre eles é sempre a vontade de querer saber como funcionam as coisas — e aplicar uma atitude que aprendi a assumir fazendo jornalismo de ciência, que é não ter medo de fazer perguntas, mesmo que pareçam básicas.","email":"clara.barata@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"clara-barata","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Clara_Barata.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737759?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#97;&#114;&#97;&#98;&#46;&#97;&#114;&#97;&#108;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/05/26/ciencia/noticia/paul-parkman-19322024-cientista-inventou-vacina-rubeola-2091701","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Obituário","url":"https://www.publico.pt/2024/05/26/ciencia/noticia/paul-parkman-19322024-cientista-inventou-vacina-rubeola-2091701"},"id":20917
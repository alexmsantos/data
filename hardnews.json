":false,"homeBrand":null,"brand":null,"rubric":{"name":"Autárquicas 2025","url":"https://www.publico.pt/2025/10/13/politica/noticia/presidencias-camara-eleitos-juntas-cdu-sofre-quebra-transversal-2150637"},"id":2150637,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Os vencedores e os vencidos das eleições autárquicas","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2037261?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3520,"nome":"David Santiago","descricao":"","email":"david.santiago@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editor de Política","profissaoNaAltura":"Editor de Política","slug":"david-santiago","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"David Santiago.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1765612?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#103;&#97;&#105;&#116;&#110;&#97;&#115;&#46;&#100;&#105;&#118;&#97;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":24,"description":"Do lado dos vencedores, o grande destaque foi para Luís Montenegro. Do lado dos vencidos, é André Ventura o rosto mais visível. Já José Luís Carneiro teve quase tanto de vencido como de vencedor.","url":"/2025/10/13/politica/noticia/vencedores-vencidos-eleicoes-autarquicas-2150575","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Autárquicas 2025","url":"https://www.publico.pt/2025/10/13/politica/noticia/vencedores-vencidos-eleicoes-autarquicas-2150575"},"id":2150575,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

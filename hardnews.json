do encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/06/14/azul/noticia/novos-prazos-dessalinizadora-algarve-barragens-pisao-pomarao-2136634","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Programa do Governo","url":"https://www.publico.pt/2025/06/14/azul/noticia/novos-prazos-dessalinizadora-algarve-barragens-pisao-pomarao-2136634"},"id":2136634,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Tintas que transformam o mundo","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2006261?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"https://www.publico.pt/estudiop/conteudo_patrocinado/tintas-CIN","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Conteúdo Comercial","url":"/estudiop"},"id":27058,"type":"EXTERNOS","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Socialistas pressionam Sánchez para que antecipe eleições legislativas","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2008017?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6310,"nome":"Inês Chaíça","descricao":"","email":"ines.chaica@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"s
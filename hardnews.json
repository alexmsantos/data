":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sergio-anibal","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Anibal.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737905?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#97;&#98;&#105;&#110;&#97;&#46;&#111;&#105;&#103;&#114;&#101;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"description":"Novo indicador do Banco de Portugal, calculado com base nos dados da Segurança Social, revela queda acentuada do saldo migratório de estrangeiros, depois dos máximos atingidos em 2023.","url":"/2025/12/12/economia/noticia/entrada-trabalhadores-estrangeiros-caiu-40-segunda-metade-2024-2157871","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Imigração","url":"https://www.publico.pt/2025/12/12/economia/noticia/entrada-trabalhadores-estrangeiros-caiu-40-segunda-metade-2024-2157871"},"id":2157871,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":26,"title":"A contagem continua. Moedas diz agora ter entregado 3000 chaves de casas em Lisboa","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2052046?tp=UH&db=IMAGENS&type=JPG","author":[{"id":217,"nome":"Samuel Alemão","descricao":"","email":"smalemao@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"samuel-alemao","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Samuel Alemao.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1990666?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":f
ovo, com ideias e algo de semelhante se passa na ciência. Depois, editei o caderno P2 e coordenei os suplementos do jornal. Actualmente, sou jornalista da Cultura.","email":"isabel.salema@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"isabel-salema","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Isabel_Salema.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737807?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#109;&#101;&#108;&#97;&#115;&#46;&#108;&#101;&#98;&#97;&#115;&#105;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/07/03/culturaipsilon/noticia/benjamin-weil-nao-vai-permanecer-frente-centro-arte-moderna-gulbenkian-2138805","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Artes","url":"https://www.publico.pt/2025/07/03/culturaipsilon/noticia/benjamin-weil-nao-vai-permanecer-frente-centro-arte-moderna-gulbenkian-2138805"},"id":2138805,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":27,"title":"A nuvem de fogo com 14 quilómetros de altura que avançou rapidamente em Espanha","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2012439?tp=UH&db=IMAGENS&type=JPG","author":[{"id":7026,"nome":"Tiago Ramalho","descricao":"","email":"tiago.ramalho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"tiago-ramalho","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"tiagoramalho.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1328997?tp=UH&db=IMAGENS&type=PNG","i
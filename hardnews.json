ro. Chama-se \"Os Anjos Não Comem Chocolate\" e tem como ponto de partida uma reportagem que fiz no PÚBLICO sobre pais que perderam os seus filhos. Ao longo dos últimos anos desempenhei funções de editora do P2, editora da Sociedade, editora-executiva e directora-adjunta.","email":"asanches@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactora principal","profissaoNaAltura":"Redactora principal","slug":"andreia-sanches","imagem":{"w":225,"h":225,"largura":225,"altura":225,"nome":"andreia-sanches-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1748990?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":"Andreia C. Faria","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#104;&#99;&#110;&#97;&#115;&#97;","contribuicao":"texto","tipo":"INTERNO"},{"id":3977,"nome":"Paulo Pimenta","descricao":"","email":"paulo.pimenta@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"paulo-pimenta","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#110;&#101;&#109;&#105;&#112;&#46;&#111;&#108;&#117;&#97;&#112;","contribuicao":"","tipo":"INTERNO"},{"id":3976,"nome":"Nuno Ferreira Santos","descricao":"","email":"nuno.santos@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"nuno-ferreira-santos","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#111;&#116;&#110;&#97;&#115;&#46;&#111;&#110;&#117;&#110;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":0,"descript
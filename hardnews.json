res (da Fugas), vivendo os automóveis sob uma perspectiva menos técnica e mais prazenteira. Desde 2009 que integro a secção de Política e tento manter debaixo de olho o panorama dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/10/politica/noticia/jose-luis-carneiro-candidato-secretariogeral-ps-2069689","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PS","url":"https://www.publico.pt/2023/11/10/politica/noticia/jose-luis-carneiro-candidato-secretariogeral-ps-2069689"},"id":2069689,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":28,"title":"Análise: Marcelo só demite Costa em Dezembro para ter Orçamento","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1857636?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3520,"nome":"David Santiago","descricao":"","email":"david.santiago@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"david-santiago","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"David Santiago.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1765612?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"
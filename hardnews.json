ovebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1747423?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Cannes","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#97;&#109;&#97;&#99;&#46;&#111;&#99;&#115;&#97;&#118;","contribuicao":"","tipo":"INTERNO"}],"shares":44,"url":"/2024/04/29/culturaipsilon/noticia/outsiders-3-edicao-12-filmes-familias-infelizes-maneira-2088463","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Cinema","url":"https://www.publico.pt/2024/04/29/culturaipsilon/noticia/outsiders-3-edicao-12-filmes-familias-infelizes-maneira-2088463"},"id":2088463,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

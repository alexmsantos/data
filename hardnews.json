ltimos anos, a crise financeira internacional e a crise da dívida soberana europeia têm ocupado grande parte do meu tempo e atenção.","email":"sergio.anibal@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sergio-anibal","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Anibal.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737905?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#97;&#98;&#105;&#110;&#97;&#46;&#111;&#105;&#103;&#114;&#101;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/08/economia/noticia/banco-portugal-pede-ritmo-reducao-divida-nao-abrande-2106975","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Finanças públicas","url":"https://www.publico.pt/2024/10/08/economia/noticia/banco-portugal-pede-ritmo-reducao-divida-nao-abrande-2106975"},"id":2106975,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":4,"title":"Pensionistas começam hoje a receber bónus","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1944442?tp=UH&db=IMAGENS&type=JPG","author":[{"id":9556,"nome":"PÚBLICO e Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico-e-lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"EXTERNOP3"}],"shares":0,"url":"/2024/10/08/economia/perguntaserespostas/pensionistas-comecam-hoje-receber-bonus-2106967","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Perguntas e Respostas","url":"/pensoes"},"
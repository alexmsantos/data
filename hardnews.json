:[{"id":1083,"nome":"Ana Brito","descricao":"","email":"ana.brito@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"ana-brito","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#116;&#105;&#114;&#98;&#46;&#97;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/19/economia/noticia/navios-mercadorias-trocam-canal-suez-cabo-boa-esperanca-2074228","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Comércio","url":"https://www.publico.pt/2023/12/19/economia/noticia/navios-mercadorias-trocam-canal-suez-cabo-boa-esperanca-2074228"},"id":2074228,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Texas reclama controlo da fronteira com o México e enfrenta Governo federal dos EUA","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1868219?tp=UH&db=IMAGENS&type=JPG","author":[{"id":132,"nome":"Alexandre Martins","descricao":"Poucos jornalistas podem começar uma autobiografia desta forma: \"Fui redactor, editor e director numa publicação, com a qual eliminei toda a concorrência.\" É verdade que já não me revejo na construção de monopólios e que tudo aconteceu quando o meu corpo ainda nem tinha acomodado a adolescência. Era uma revista sobre um jogo a que chamávamos futebol-carica. Textos martelados numa máquina de escrever e fotografias recortadas de revistas. Tudo colado, fotocopiado e agrafado. Entre essa primeira experiência no jornalismo e a minha entrada no PÚBLICO, em 1999, aconteceram muitas coisas. Hoje sou jornalista na secção Mundo. Fui redactor e editor do PÚBLICO Online, editor de Multimédia e de Redes Sociais. Gosto de escrever sobre tecnologia. Gosto de fazer muitas coisas, muito diferentes. Como disse o comediante norte-americano Bill Hicks: “It’s just a ride.” “E tudo isso, que é tanto, é pouco para o que eu quero.” Só não gosto de usar expressões em inglês e de citar Fernando Pessoa.","email":"alexandre.martins@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"alexandre-martins","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Alexandre_Martins.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1087168?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#110;&#105;&#116;&#114;&#97;&#109;&#46;&#101;&#114;&#100;&#110;&#97;&#120;&#101;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":1,"url":"/2023/12/19/mundo/noticia/texas-reclama-controlo-fronteira-mexico-enfrenta-governo-federal-eua-2074206","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"EUA","url":"https://www.publico.pt/2023/12/19/mundo/noticia/texas-reclama-controlo-fronteira-mexico-enfrenta-governo-federal-eua-2074206"},"id":2074206,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Mota-Engil vende negócio de construção e imobiliário na Polónia por 90 milhões","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1868335?tp=UH&db=IMAGENS&type=JPG","author":[{"id":15415,"nome":"Rafaela Burd Relvas","descricao":"","email":"rafaela.relvas@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"rafaela-burd-relvas","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#118;&#108;&#101;&#114;&#46;&#97;&#108;&#101;&#97;&#102;&#97;&#114;","contribu
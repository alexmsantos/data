[{"pos":0,"title":"Portugal, o país onde ainda não é fácil largar o carro","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1863793?tp=UH&db=IMAGENS&type=JPG","author":[{"id":4119,"nome":"Camilo Soldado","descricao":"","email":"camilo.soldado@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"camilo-soldado","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#97;&#100;&#108;&#111;&#115;&#46;&#111;&#108;&#105;&#109;&#97;&#99;","contribuicao":"texto","tipo":"INTERNO"},{"id":217,"nome":"Samuel Alemão","descricao":"","email":"smalemao@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"samuel-alemao","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#97;&#109;&#101;&#108;&#97;&#109;&#115;","contribuicao":"texto","tipo":"INTERNO"},{"id":3917,"nome":"Nelson Garrido","descricao":"Nasci em Vila Nova de Gaia, Portugal. Em 1996 concluí o bacharelato em Tecnologias da Comunicação Audiovisual. No mesmo ano, frequentei uma formação avançada em fotografia na escola Karel de Grote-Hogescholl Antwerpen, na Bélgica, e fiz um estágio em fotografia digital e de grande formato no Studio Brison, também na Bélgica. Em Portugal, trabalhei em regime de freelancer com várias revistas, na área da fotografia de reportagem e de arquitectura. Em 2000, concluí a licenciatura em Comunicação Social e em 2005 a Licenciatura em Fotografia. Em 2005, ganhei três menções honrosas no concurso de fotojornalismo da revista Visão e venci a categoria de fotografia de arquitectura do Euro Press Photo Awards da Fuji Film. Fui premiado no Prémio Estação Imagem|Mora em 2010 e 2011. Desde 1999, sou fotojornalista no PÚBLICO.","email":"ngarrido@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"nelson-garrido","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Nelson_Garrido.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737874?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#105;&#114;&#114;&#97;&#103;&#110;","contribuicao":"fotografia","tipo":"INTERNO"},{"id":14064,"nome":"Ricardo Lopes","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"ricardo-lopes","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"fotografia","tipo":"EXTERNO"}],"shares":0,"url":"/2023/12/01/local/reportagem/portugal-pais-onde-nao-facil-largar-carro-2072167","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/mobilidade"},"id":2072167,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"Cinco promissores projectos de investigação distinguidos no Prémio Maria de Sousa","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1862495?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":12,"url":"/2023/11/28/estudiop/noticia/cinco-promissores-projectos-investigacao-distinguidos-premio-maria-sousa-2071729","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Prémio Maria de Sousa 2023","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1862476?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2023/11/28/estudiop/noticia/cinco-prom
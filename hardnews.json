ise financeira internacional e a crise da dívida soberana europeia têm ocupado grande parte do meu tempo e atenção.","email":"sergio.anibal@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sergio-anibal","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Anibal.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737905?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#97;&#98;&#105;&#110;&#97;&#46;&#111;&#105;&#103;&#114;&#101;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/09/23/economia/noticia/contas-bastante-piores-excedente-alto-apenas-quatro-meses-2104932","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Orçamento do Estado 2024","url":"https://www.publico.pt/2024/09/23/economia/noticia/contas-bastante-piores-excedente-alto-apenas-quatro-meses-2104932"},"id":2104932,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":5,"title":"Nasce em Outubro portal para reportar indisciplina que “não sai” dos muros da escola","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1938907?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11423,"nome":"Daniela Carmo","descricao":"","email":"daniela.carmo@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"daniela-carmo","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#109;&#114;&#97;&#99;&#46;&#97;&#108;&#101;&#105;&#110;&#97;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/09/23/sociedade/noticia/na
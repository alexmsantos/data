Rubric":false,"tag":null,"size":30},{"pos":29,"title":"Série especial “Da minha língua vê-se o mundo”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1980559?tp=UH&db=IMAGENS&type=GIF","author":[{"id":5750,"nome":"Francisco Lopes","descricao":"","email":"francisco.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista infográfico","profissaoNaAltura":"Jornalista infográfico","slug":"francisco-f-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"francisco-lopes-foto.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1276822?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#111;&#99;&#115;&#105;&#99;&#110;&#97;&#114;&#102;","contribuicao":null,"tipo":"INTERNO"},{"id":3929,"nome":"José Alves","descricao":"Já na escola primária que frequentei havia sido colocado em lugar de destaque um desenho que fiz, que explicava como os bombeiros apagavam os fogos. O desenho sempre foi uma paixão, mas, a partir de determinada altura da minha juventude, o objectivo passou a ser mais do que a forma.\r\nEste percurso levou-me ao mundo da infografia em 1996. O departamento denominava-se Investigação e Desenvolvimento e o que fazíamos ainda era naturalmente confundido com “o pessoal que faz bonecos”, alguma publicidade e com uma larga visão para a aventura em cada trabalho.\r\nEstou no PÚBLICO desde 2006 e hoje vejo que é maior a visão do que a aventura. Todos os dias me chegam às mãos os mais variados documentos. Folhas de Excel, documentos em Word ou mesmo rabiscos em folhas A4 acompanhadas de outros tantos gráficos de linhas e barras. A infografia é isto! Interpretar, avaliar, pesquisar e transformar folhas de Excel em informação gráfica (infografia) fácil, clara e objectiva aos olhos do leitor.\r\n","email":"jose.alves@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista Infográfico","profissaoNaAltura":"Jornalista Infográfico","slug":"jose-alves","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Jose_Alves.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737831?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#118;&#108;&#97;&#46;&#101;&#115;&#111;&#106;","contribuicao":null,"tipo":"INTERNO"},{"id":10403,"nome":"Marco Neves","descricao":"Marco Neves é tradutor e docente universitário. Escreve no blogue Certas Palavras","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"","profissaoNaAltura":"","slug":"marco-neves","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":null,"tipo":"EXTERNOP3"}],"shares":20,"url":"/2025/02/26/infografia/lingua-vese-mundo-853","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PÚBLICO 35 anos","url":"https://www.publico.pt/2025/02/26/infografia/lingua-vese-mundo-853"},"id":853,"type":"INFOGRAFIA","isMultipleRubric":false,"tag":null,"size":30}]

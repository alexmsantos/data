publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sergio-c-andrade","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Andrade.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737904?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#101;&#100;&#97;&#114;&#100;&#110;&#97;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":4,"url":"/2025/02/08/culturaipsilon/noticia/historia-matosinhos-desde-porto-leixoes-ate-boa-nova-cabo-mundo-2121449","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Livros","url":"https://www.publico.pt/2025/02/08/culturaipsilon/noticia/historia-matosinhos-desde-porto-leixoes-ate-boa-nova-cabo-mundo-2121449"},"id":2121449,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":25,"title":"Vinho do Ano é Baga de Luís Pato. <em>Revista de Vinhos</em> premiou Melhores de 2024","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1976223?tp=UH&db=IMAGENS&type=JPG","author":[{"id":16085,"nome":"Terroir","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"terroir","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/02/07/terroir/noticia/vinho-ano-baga-pe-franco-luis-pato-revista-vinhos-premiou-melhores-2024-2121750","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"fugas"},"brand":null,"rubric":{"name":"Prémios","url":"https://www.publico.pt/2025/02/07/terroir/noticia/vinho-ano-baga-pe-franco-luis-pato-revista-vinhos-premiou-melhores-2024-2121750"},"id":2121750,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":26,"title":"A incrível história de Lou Reed, o músico que deu mundos ao rock, um homem na cidade","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1975308?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1034,"nome":"Mário Lopes","descricao":"A culpa é de um gira-discos e dos discos que o meu pai rodava no gira-discos. A música chegou primeiro. Como com a maioria dos adolescentes, a música moldou-me gostos e atitudes, os discos foram guias para a descoberta do mundo. Através deles, cheguei a livros, a cinema, a nomes e mais nomes e a outros discos, outros livros, outros filmes. Foi pela música que cheguei ao jornalismo. Primeiro no Blitz, onde entrei estagiário no final do curso de Ciências da Comunicação e que seria, posteriormente, a casa do meu primeiro emprego. Depois no Diário de Notícias, onde a música começou a significar mais jornalismo, aberto a mais que bandas e discos. Ao PÚBLICO cheguei em 2005. Para escrever maioritariamente sobre música. Para, através da música, chegar a outros universos da cultura. E para, através da cultura, tentar compreender melhor o mundo e, escrevendo sobre ela, tentar mostrá-lo o mais claramente possível. A adolescência já lá vai há muito. Discos, livros, filmes, continuam a ser guias de descoberta. Assim sendo, continuarei.","email":"mario.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"mario-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Mario_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/860961?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#111;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/02/08/culturaipsilon/entrevista/lou-reed-musico-deu-novos-mundos-rock-homem-cidade-2120824","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/livros"},"id":2120824,"type":"NOTICIA","isMultipleRubric":true,"tag":{"id":712,"nome":"Livros","slug":"livros","tagEn":"","isPrincipal":false,"isVisible":true,"urlRoute":"/tlivros","isPrincipalParaArtigo":true,"isTimeLine":false,"forcaConteudoAberto":false,"follow_type":null,"descricao":null,"imagem":null,"html":null,"isForForuns":true,"isUsedInForuns":false},"size":30},{"pos":27,"title":"Uma casa entre o contemporâneo e o tradicional, a olhar para a Serra do Louro","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1976354?tp=UH&db=IMAGENS&type=JPG","author":[{"id":18324,"nome":"Natalia Vásquez","descricao":"","email":"natalia.vasquez@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista estagiária","profissaoNaAltura":"Jornalista estagiária","slug":"natalia-vasquez","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#122;&#101;&#117;&#113;&#115;&#97;&#118;&#46;&#97;&#105;&#108;&#97;&#116;&#97;&#110;","contribuicao":null,"tipo":"INTERNO"}],"shares":0,"url":"/2025/02/08/p3/fotogaleria/uma-casa-entre-contemporaneo-tradicional-olhar-serra-louro-413160","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"p3"},"brand":null,"rubric":{"name":"Arquitectura","url":"https://www.publico.pt/2025/02/08/p3/fotogaleria/uma-casa-entre-contemporaneo-tradicional-olhar-serra-louro-413160"},"id":413160,"type":"FOTOGALERIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":28,"title":"O que não falta são espaços para noctívagos. “A malta gosta de estudar à noite”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1976148?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6052,"nome":"Teresa Serafim","descricao":"Sempre gostei de histórias – de as ouvir e de as contar. Durante a adolescência, a ler jornais e revistas, percebi que o jornalismo era o meio ideal para o fazer. Na licenciatura de Ciências da Comunicação na Faculdade de Ciências Sociais e Humanas da Universidade Nova de Lisboa, com algumas experiências em sites e no jornal da minha terra (Sousel), tive a certeza de que ser jornalista era mesmo o que queria ser. Estou no PÚBLICO desde 2016 a escrever diariamente para a secção de Ciência. Aqui, escuto e reconto algumas das histórias mais bonitas, desafiantes e inspiradoras.","email":"teresa.serafim@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"teresa-serafim","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Teresa Serafim.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1686947?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#109;&#105;&#102;&#97;&#114;&#101;&#115;&#46;&#97;&#115;&#101;&#114;&#101;&#116;","contribuicao":"texto","tipo":"INTERNO"},{"id":3976,"nome":"Nuno Ferreira Santos","descricao":"","email":"nuno.santos@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"Fotojornalista","slug":"nuno-ferreira-santos","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#111;&#116;&#110;&#97;&#115;&#46;&#111;&#110;&#117;&#110;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":0,"url":"/2025/02/07/local/reportagem/nao-falta-sao-espacos-noctivagos-cidades-farr
 dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"},{"id":7253,"nome":"Susana Madureira Martins (Renascença)","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Renascença","profissaoNaAltura":"Renascença","slug":"susana-madureira-martins","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"},{"id":3983,"nome":"Rui Gaudêncio","descricao":"","email":"rgaudencio@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"rui-gaudencio","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#105;&#99;&#110;&#101;&#100;&#117;&#97;&#103;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":36,"url":"/2024/05/24/politica/entrevista/marta-temido-reforma-aima-provavelmente-maneira-2091642","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/europeias-2024-entrevistas"},"id":2091642,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":19,"title":"Xavi foi despedido do comando técnico do Barcelona","isPremium":false,"image":"https://imagens.pu
":[{"id":17765,"nome":"Ana Sofia Malheiro","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"sofia-malheiro","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"INTERNO"},{"id":177,"nome":"Andréia Azevedo Soares","descricao":"Nasci no Rio de Janeiro, onde estudei jornalismo na Universidade Federal. Mudei-me para Portugal em 1999. Trabalho no PÚBLICO desde 2000, com duas interrupções – uma para um doutoramento em Comunicação de Ciência no Imperial College, em Londres, e outra para colaborar com a revista científica WHO Bulletin, em Genebra. Já fui estagiária, colaboradora, repórter e sub-editora em diferentes secções deste jornal. A ciência e a literatura são as áreas sobre as quais mais gosto de escrever; fascina-me o modo como o escritor e o cientista se empenham para detalhar e compreender o mundo que nos rodeia, incluindo os seres vivos que nele habitam.\r\n\r\n\r\n\r\n","email":"aasoares@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"andreia-azevedo-soares","imagem":{"w":null,"h":null,"largura":501,"altura":498,"nome":"andreia_soares-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1725026?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#114;&#97;&#111;&#115;&#97;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":15,"url":"/2023/12/14/azul/noticia/activistas-climaticos-penduramse-viaduto-duarte-pacheco-lisboa-2073656","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Protesto","url":"https://www.publico.pt/2023/12/14/azul/notici
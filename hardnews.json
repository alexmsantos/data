al de 2015, cheguei ao PÚBLICO para estagiar na secção Local, onde descobri algumas das histórias que Lisboa tem para contar. Abraçando o desafio online, passei a fazer parte da equipa fundadora do Azul, o nosso projecto dedicado à crise climática. E aprendo todos os dias.","email":"claudia.silva@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"claudia-carvalho-silva","imagem":{"w":null,"h":null,"largura":437,"altura":572,"nome":"CLAUDIACARVALHOSILVA_REDIMENSIONADA-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1440637?tp=UH&db=IMAGENS&type=PNG","is_vertical":true,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#118;&#108;&#105;&#115;&#46;&#97;&#105;&#100;&#117;&#97;&#108;&#99;","contribuicao":"texto","tipo":"INTERNO"},{"id":3920,"nome":"Daniel Rocha","descricao":"","email":"drocha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"daniel-rocha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Daniel_Rocha.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737766?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#104;&#99;&#111;&#114;&#100;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":54,"url":"/2025/04/20/p3/reportagem/ha-equipa-tratar-fobias-realidade-virtual-estranho-funciona-2130191","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"p3"},"brand":null,"rubric":{"name":"Reportagem","url":"/saude-mental"},"id":2130191,"type":"NOTI
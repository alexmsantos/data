ooglePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"pedro-crisostomo","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Pedro Crisostomo.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530342?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#109;&#111;&#116;&#115;&#111;&#115;&#105;&#114;&#99;&#46;&#111;&#114;&#100;&#101;&#112;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/20/economia/noticia/fisco-nao-aplicar-lei-reduz-coimas-portagens-2108538","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Auto-estradas","url":"https://www.publico.pt/2024/10/20/economia/noticia/fisco-nao-aplicar-lei-reduz-coimas-portagens-2108538"},"id":2108538,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":9,"title":"Dentro do grupo de Telegram português onde 70 mil pessoas trocam imagens de mulheres","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1947347?tp=UH&db=IMAGENS&type=PNG","author":[{"id":11076,"nome":"Mariana Durães","descricao":"","email":"mariana.duraes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"mariana-duraes","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#97;&#114;&#117;&#100;&#46;&#97;&#110;&#97;&#105;&#114;&#97;&#109;","contribuicao":"texto","tipo":"INTERNO"},{"id":3929,"nome":"José Alves","descricao":"Já na escola primária que frequentei havia sido colocado em lugar de destaque um desenho que fiz, que explicava como os bombeiros apagavam os fogos. O desenho sempre foi uma paixão, mas, a partir de determinada altura da minha juventude, o objectivo passou a ser mais do que a forma.\r\nEste percurso levou-me ao mundo da infografia em 1996. O departamento denominava-se Investigação e Desenvolvimento e o que fazíamos ainda era naturalmente confundido com “o pessoal que faz bonecos”, alguma publicidade e com uma larga visão para a aventura em cada trabalho.\r\nEstou no PÚBLICO desde 2006 e hoje vejo que é maior a visão do que a aventura. Todos os dias me chegam às mãos os mais variados documentos. Folhas de Excel, documentos em Word ou mesmo rabiscos em folhas A4 acompanhadas de outros tantos gráficos de linhas e barras. A infografia é isto! Interpretar, avaliar, pesquisar e transformar folhas de Excel em informação gráfica (infografia) fácil, clara e objectiva aos olhos do leitor.\r\n","email":"jose.alves@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Infografia","profissaoNaAltura":"","slug":"jose-alves","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Jose_Alves.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737831?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#118;&#108;&#97;&#46;&#101;&#115;&#111;&#106;","contribuicao":"ilustração","tipo":"INTERNO"},{"id":18560,"nome":"Catarina Póvoa","descricao":"Fotógrafa","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotógrafa","profissaoNaAltura":"","slug":"catarina-povoa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/20/p3/reportagem/entramos-grupo-telegram-portugues-onde-70-mil-pessoas-trocam-imagens-mulheres-2106021","comments":0,"spoonsore
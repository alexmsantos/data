[{"pos":0,"title":"Buscas da PSP ao FC Porto: milhares de bilhetes apreendidos e 13 arguidos\r\n\r\n","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1907642?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11964,"nome":"Sónia Trigueirão","descricao":"","email":"sonia.trigueirao@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sonia-trigueirao","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#97;&#114;&#105;&#101;&#117;&#103;&#105;&#114;&#116;&#46;&#97;&#105;&#110;&#111;&#115;","contribuicao":"","tipo":"INTERNO"},{"id":3641,"nome":"Pedro Sales Dias","descricao":"Comecei no PÚBLICO quando o papel ainda dominava quase sem concorrência as redacções. Passei depois pelo jornal <em>online Portugal Diário</em>, pelo <em>Correio da Manhã</em> – onde comecei a especializar-me nos temas de Polícia e Justiça, pelo jornal <em>i</em>, <em> Semanário Grande Porto </em> e <em> Jornal de Notícias</em>. E voltei ao PÚBLICO por duas vezes e por aqui continuo. Após alguns anos a escrever sobre Justiça e Polícia – entre outros temas como Política, Local e Economia – passei a editor da maior secção deste diário. Durante oito anos editei a Sociedade, uma secção central no dia-a-dia do jornal. Fui também editor de fecho do jornal. Em Outubro de 2023, assumi funções como editor de <em> Online</em>, uma área fundamental para um jornal de referência como o PÚBLICO. Gosto de policiais – nos livros e nos grandes e pequenos ecrãs – e mantenho o interesse em assuntos de Justiça. Também mantenho a Saúde no meu olhar, não tivesse sido socorrista na Cruz Vermelha Portuguesa, onde durante dez anos tripulei dezenas de ambulâncias em milhares de serviços de emergência accionados pelo 112/INEM. Acredito que não há história que um diário de referência não possa contar, desde que o faça bem e com rigor.","email":"pedro.dias@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editor de Online","profissaoNaAltura":"","slug":"pedro-sales-dias","imagem":{"w":null,"h":null,"largura":500,"altura":500,"nome":"copy_783053384.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1545651?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#105;&#100;&#46;&#111;&#114;&#100;&#101;&#112;","contribuicao":"","tipo":"INTERNO"}],"shares":84,"url":"/2024/05/12/sociedade/noticia/fc-porto-revela-alvo-buscas-2090145","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Justiça","url":"https://www.publico.pt/2024/05/12/sociedade/noticia/fc-porto-revela-alvo-buscas-2090145"},"id":2090145,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"Lena d’Água, Jorge Palma, Pedro Abrunhosa: o FNAC Live está de volta","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1906851?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":35,"url":"/2024/05/09/estudiop/noticia/lena-dagua-jorge-palma-pedro-abrunhosa-fnac-live-volta-2089708","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"FNAC","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1574680?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2024/05/09/estudiop/noticia/lena-dagua-jorge-palma-pedro-abrunhosa-fnac-live-volta-2089708"},"id":2089708,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":2,"title":"Livre, o “europeísta”, olhou mais para dentro do que para Bruxelas","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1907680?tp=UH&db=IMAGENS&type=JPG"
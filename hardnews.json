rtical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#114;&#97;&#111;&#115;&#97;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/04/10/azul/noticia/pfas-fontes-agua-globais-contaminadas-substancias-quimicas-eternas-2086403","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Poluição","url":"https://www.publico.pt/2024/04/10/azul/noticia/pfas-fontes-agua-globais-contaminadas-substancias-quimicas-eternas-2086403"},"id":2086403,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Decisão do Tribunal Europeu abre portas à justiça climática","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1898843?tp=UH&db=IMAGENS&type=JPG","author":[{"id":10,"nome":"Rita Siza","descricao":"","email":"rita.siza@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":", em Estrasburgo","profissaoActual":"correspondente em Bruxelas","profissaoNaAltura":"correspondente em Bruxelas","slug":"rita-siza","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Rita_Siza.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737896?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#122;&#105;&#115;&#46;&#97;&#116;&#105;&#114;","contribuicao":"","tipo":"INTERNO"},{"id":6488,"nome":"Aline Flor","descricao":"Natural do Rio de Janeiro, vivo em Portugal há mais de 25 anos. Criei raízes. Fui directora do Jornal Universitário do Porto, passei pela Ciência e Tecnologia no Canal Superior, editei notícias do portal AEIOU.pt. Fiz um mestrado em Cinema Documental que me abriu os olhos (e os ouvidos) para a riqueza de cada encontro com o outro. Cheguei ao PÚBLICO para integrar a equipa de áudio, onde produzi podcasts em áreas desde a igualdade de género às questões europeias. Hoje faço parte do Azul, a nossa secção dedicada à crise climática e sustentabilidade. Gosto de ouvir e de contar as histórias que ficam à margem e acredito que temos sempre algo a aprender quando encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":1,"url":"/2024/04/09/azul/noticia/nao-derrubamos-muro-fizemos-fenda-decisao-tribunal-europeu-abre-portas-justica-climatica-2086414","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Clima","url":"https://www.publico.pt/2024/04/09/azul/noticia/nao-derrubamos-muro-fizemos-fenda-decisao-tribunal-europeu-abre-portas-justica-climatica-2086414"},"id":2086414,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"Parlamento Europeu vota reforma da política de migração e asilo da UE","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1898951?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":f
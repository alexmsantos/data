am em árabe.","email":"slorena@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":", em Mossul","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sofia-lorena","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"sofia-lorena-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739860?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#101;&#114;&#111;&#108;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":30,"url":"/2023/12/30/mundo/noticia/marido-fatima-ima-daesh-durante-seis-meses-tiroulhe-vida-2075266","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Iraque","url":"https://www.publico.pt/2023/12/30/mundo/noticia/marido-fatima-ima-daesh-durante-seis-meses-tiroulhe-vida-2075266"},"id":2075266,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Euribor a 12 meses anula sete meses de subidas, prestações com menor aumento do ano","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1870969?tp=UH&db=IMAGENS&type=JPG","author":[{"id":459,"nome":"Rosa Soares","descricao":"","email":"rsoares@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"rosa-soares","imagem":{"w":null,"h":null,"largura":472,"altura":472,"nome":"rosa soares.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1390182?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#114;&#97;&#111;&#115;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/30/economia/noticia/euribor-12-meses-anula-sete-meses-subidas-prestacoes-menor-aumento-ano-2075244","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Juros","url":"https://www.publico.pt/2023/12/30/economia/noticia/euribor-12-meses-anula-sete-meses-subidas-prestacoes-menor-aumento-ano-2075244"},"id":2075244,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Como funciona o prémio salarial para trabalhadores recém-licenciados?","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1870930?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3150,"nome":"Raquel Martins","descricao":"Aos 15 anos decidi que queria mudar o mundo e, para desgosto da minha professora de Português, quis ser jornalista. Fiz o curso de Ciências da Comunicação na Universidade Nova, frequentei o Cenjor e andei indecisa entre a rádio e a imprensa. Depois de uma breve passagem pela rádio (Antena 3 e Rádio Expo), acabei por entrar no jornalismo económico um pouco por acaso. Primeiro, no Diário Económico – onde escrevi sobre ambiente, autarquias, política e, mais tarde, sobre trabalho e função pública – depois no Jornal de Negócios. Foi por isso que, em 2009, fiz uma formação na Universidade Católica sobre legislação laboral. Estou no PÚBLICO desde 2010. Acompanho os assuntos relacionados com trabalho, emprego e administração pública (há quem diga, com humor, que o Código do Trabalho é o meu livro de cabeceira.) Defendo que o jornalismo tem um papel cada vez mais importante na descodificação da avalanche de informação que todos os dias circula nas mais diversas plataformas.","email":"raquel.martins@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"raquel-martins","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#6
":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#115;&#111;&#100;&#114;&#97;&#99;&#46;&#97;&#110;&#97;&#111;&#106;","contribuicao":"","tipo":"INTERNO"}],"shares":18,"url":"/2023/10/10/sociedade/noticia/paredes-voltaram-falar-ces-serio-estao-processar-fez-denuncias-2066206","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Assédio sexual","url":"https://www.publico.pt/2023/10/10/sociedade/noticia/paredes-voltaram-falar-ces-serio-estao-processar-fez-denuncias-2066206"},"id":2066206,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

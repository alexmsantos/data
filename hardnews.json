onalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/01/azul/noticia/guterres-avisa-industrias-fosseis-vosso-papel-tornarse-obsoleto-2072205","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"COP28","url":"https://www.publico.pt/2023/12/01/azul/noticia/guterres-avisa-industrias-fosseis-vosso-papel-tornarse-obsoleto-2072205"},"id":2072205,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":8,"title":"1.º de Dezembro é “talvez a data mais celebrada a seguir ao 25 de Abril”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1863646?tp=UH&db=IMAGENS&type=JPG","author":[{"id":14893,"nome":"Mariana Marques Tiago","descricao":"","email":"mariana.tiago@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"mariana-marques-tiago","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#103;&#97;&#105;&#116;&#46;&#97;&#110;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/01/politica/noticia/1-dezembro-ja-nao-moribundo-data-celebrada-seguir-25-abril-2072160","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Restauração da Independência","url":"https://www.publico.pt/2023/12/01/politica/noticia/1-dezembro-ja-nao-moribundo-data-celebrada-seguir-25-abril-2072160"},"id":2072160,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":9,"title":"De uma janela na Arrentela, Soraia Ramos fez-se cantora","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1862971?tp=UH&db=IMAGENS&type=JPG","author":[{"id":4961,"nome":"Mariana Duarte","descricao":"","email":"mariana.duarte@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Texto","slug":"mariana-duarte","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#101;&#116;&#114;&#97;&#117;&#100;&#46;&#97;&#110;&#97;&#105;&#114;&#97;&#109;","contribuicao":"texto","tipo":"INTERNO"},{"id":14064,"nome":"Ricardo Lopes","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"Fotografia","slug":"ricardo-lopes","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"fotografia","tipo":"EXTERNO"}],"shares":2,"url":"/2023/12/01/culturaipsilon/reportagem/janela-arrentela-soraia-ramos-fezse-cantora-espalhar-cultura-cabo-verde-2071861","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/musica"},"id":2071861,"type":"NOTICIA","isMultipleRubric":true,"tag":{"id":708,"nome":"Música","slug":"musica","tagEn":"","isPrincipal":false,"isVisible":true,"urlRoute":"/tmusica","isPrincipalParaArtigo":true,"isTimeLine":false,"forcaConteudoAberto":false,"follow_type":null,"descricao":null,"imagem":null,"html":null,"isForForuns":true,"isUsedInForuns":false},"size":30},{"pos":10,"title":"Pedro Nuno pisca olho a professores e empresários, Carneiro aposta na justiça e saúde","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1863699?tp=UH&db=IMAGENS&type=JPG","author":[{"id":157,"nome":"Maria Lopes","descricao":"Entrei para o PÚBLICO, vinda da universidade, quando se vivia a excitação das vésperas da primeira Exposição Mundial em Portugal. Calcorreei a Expo-98 de fio a pavio, de caderno, caneta e, por vezes, de Polaroid na mão. Esses poucos meses ensinaram-me a conjugar o verbo tão português “desenrascar-se”.\r\nNestes mais de 20 anos, passei depois pelas secções Local, Media (por duas vezes) e Economia. Estive no lançamento do suplemento Digital – que apesar do nome existiu apenas em papel – e pertenci à equipa do irreverente projecto Sexta, o jornal gratuito que o PÚBLICO partilhou com A Bola, em 2007 e 2008. Em simultâneo, durante dez anos, escrevi também para o Motores (da Fugas), vivendo os automóveis sob uma perspectiva menos técnica e mais prazenteira. Desde 2009 que integro a secção de Política e tento manter debaixo de olho o panorama dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/11/30/politica/noticia/pedro-nuno-pisca-olho-professores-empresarios-carneiro-aposta-justica-saude-2072184","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PS","url":"https://www.publico
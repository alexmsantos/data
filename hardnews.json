zer perguntas, mesmo que pareçam básicas.","email":"clara.barata@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"clara-barata","imagem":{"w":null,"h":null,"largura":368,"altura":331,"nome":"_RG__foto_Clara_Barata_DSCF4152-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1926383?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#97;&#114;&#97;&#98;&#46;&#97;&#114;&#97;&#108;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/09/19/azul/noticia/incendios-duplicaram-recorde-emissoes-carbono-portugal-setembro-2104676","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Incêndios florestais","url":"https://www.publico.pt/2024/09/19/azul/noticia/incendios-duplicaram-recorde-emissoes-carbono-portugal-setembro-2104676"},"id":2104676,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

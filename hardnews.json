-vaza","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Marco_Vaza.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737861?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#122;&#97;&#118;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/06/22/desporto/noticia/sporting-goleia-benfica-fica-perto-penta-futsal-2137487","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Futsal","url":"https://www.publico.pt/2025/06/22/desporto/noticia/sporting-goleia-benfica-fica-perto-penta-futsal-2137487"},"id":2137487,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

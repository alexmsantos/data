alizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"sofia-rodrigues","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sofia_Rodrigues.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1202811?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#117;&#103;&#105;&#114;&#100;&#111;&#114;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/01/31/politica/noticia/eurodeputado-nuno-melo-faz-aprovar-proibicao-amnistia-crimes-corrupcao-ue-2078790","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Parlamento Europeu","url":"https://www.publico.pt/2024/01/31/politica/noticia/eurodeputado-nuno-melo-faz-aprovar-proibicao-amnistia-crimes-corrupcao-ue-2078790"},"id":2078790,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Republicanos preparam impugnação histórica do responsável pelas fronteiras","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1879199?tp=UH&db=IMAGENS&type=JPG","author":[{"id":132,"nome":"Alexandre Martins","descricao":"Poucos jornalistas podem começar uma autobiografia desta forma: \"Fui redactor, editor e director numa publicação, com a qual eliminei toda a concorrência.\" É verdade que já não me revejo na construção de monopólios e que tudo aconteceu quando o meu corpo ainda nem tinha acomodado a adolescência. Era uma revista sobre um jogo a que chamávamos futebol-carica. Textos martelados numa máquina de escrever e fotografias recortadas de revistas. Tudo colado, fotocopiado e agrafado. Entre essa primeira experiência no jornalismo e a minha entrada no PÚBLICO, em 1999, aconteceram muitas coisas. Hoje sou jornalista na secção Mundo. Fui redactor e editor do PÚBLICO Online, editor de Multimédia e de Redes Sociais. Gosto de escrever sobre tecnologia. Gosto de fazer muitas coisas, muito diferentes. Como disse o comediante norte-americano Bill Hicks: “It’s just a ride.” “E tudo isso, que é tanto, é pouco para o que eu quero.” Só não gosto de usar expressões em inglês e de citar Fernando Pessoa.","email":"alexandre.martins@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"alexandre-martins","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Alexandre_Martins.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1087168?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#110;&#105;&#116;&#114;&#97;&#109;&#46;&#101;&#114;&#100;&#110;&#97;&#120;&#101;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":1,"url":"/2024/01/31/mundo/noticia/republicanos-preparam-impugnacao-historica-responsavel-fronteiras-2078744","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"EUA","url":"https://www.publico.pt/2024/01/31/mundo/noticia/republicanos-preparam-impugnacao-historica-responsavel-fronteiras-2078744"},"id":2078744,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Aliados de Trump prometem “guerra” contra Taylor Swift","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1879175?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11214,"nome":"Sofia Neves","descricao":"","email":"sofia.neves@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sofia-neves","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;
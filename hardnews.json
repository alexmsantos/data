sa na ciência. Depois, editei o caderno P2 e coordenei os suplementos do jornal. Actualmente, sou jornalista da Cultura.","email":"isabel.salema@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"isabel-salema","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Isabel_Salema.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737807?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#109;&#101;&#108;&#97;&#115;&#46;&#108;&#101;&#98;&#97;&#115;&#105;","contribuicao":"","tipo":"INTERNO"},{"id":7,"nome":"Luís Miguel Queirós","descricao":"","email":"lmqueiros@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"luis-miguel-queiros","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"PP LMQueiros.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/868576?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#111;&#114;&#105;&#101;&#117;&#113;&#109;&#108;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/05/14/culturaipsilon/noticia/ministra-cultura-vai-substituir-responsaveis-museus-monumentos-patrimonio-cultural-ip-2090473","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Política Cultural","url":"https://www.publico.pt/2024/05/14/culturaipsilon/noticia/ministra-cultura-vai-substituir-responsaveis-museus-monumentos-patrimonio-cultural-ip-2090473"},"id":2090473,"type":"NOTICIA","isMultipleRubric":f
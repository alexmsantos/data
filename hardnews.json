. E até aprendi a escrever Saddam em árabe.","email":"slorena@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"sofia-lorena","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"sofia-lorena-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739860?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#101;&#114;&#111;&#108;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/03/11/mundo/noticia/chacina-fome-gaza-tensoes-jerusalem-cisjordania-comecou-ramadao-2083323","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Médio Oriente","url":"https://www.publico.pt/2024/03/11/mundo/noticia/chacina-fome-gaza-tensoes-jerusalem-cisjordania-comecou-ramadao-2083323"},"id":2083323,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Susana Peralta: “Os homens não podem ser o que quiserem. Isso é uma mentira completa”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1891448?tp=UH&db=IMAGENS&type=JPG","author":[{"id":17968,"nome":"Maria Ana Barroso","descricao":"","email":"maria.ana.barroso@gmail.com","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Psicóloga","profissaoNaAltura":"Psicóloga","slug":"maria-ana-barroso","imagem":{"w":null,"h":null,"largura":484,"altura":515,"nome":"maria ana foto.jpeg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1876812?tp=UH&db=IMAGENS&type=JPG","is_vertical":true,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#109;&#111;&#99;&#46;&#108;&#105;&#97;&#109;&#103;&#64;&#111;&#115;&#11
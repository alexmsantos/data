uthor":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/01/25/economia/noticia/quatro-10-portugueses-querem-comprar-viatura-dentro-dois-tres-anos-2078075","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Automóveis","url":"https://www.publico.pt/2024/01/25/economia/noticia/quatro-10-portugueses-querem-comprar-viatura-dentro-dois-tres-anos-2078075"},"id":2078075,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Excedentes acumulados pela ADSE permitem cobrir défice por 11 anos","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1877597?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3150,"nome":"Raquel Martins","descricao":"Aos 15 anos decidi que queria mudar o mundo e, para desgosto da minha professora de Português, quis ser jornalista. Fiz o curso de Ciências da Comunicação na Universidade Nova, frequentei o Cenjor e andei indecisa entre a rádio e a imprensa. Depois de uma breve passagem pela rádio (Antena 3 e Rádio Expo), acabei por entrar no jornalismo económico um pouco por acaso. Primeiro, no Diário Económico – onde escrevi sobre ambiente, autarquias, política e, mais tarde, sobre trabalho e função pública – depois no Jornal de Negócios. Foi por isso que, em 2009, fiz uma formação na Universidade Católica sobre legislação laboral. Estou no PÚBLICO desde 2010. Acompanho os assuntos relacionados com trabalho, emprego e administração pública (há quem diga, com humor, que o Código do Trabalho é o meu livro de cabeceira.) Defendo que o jornalismo tem um papel cada vez mais importante na descodificação da avalanche de informação que todos os dias circula nas mais diversas plataformas.","email":"raquel.martins@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"raquel-martins","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#110;&#105;&#116;&#114;&#97;&#109;&#46;&#108;&#101;&#117;&#113;&#97;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":2,"url":"/2024/01/25/economia/noticia/excedentes-acumulados-adse-permitem-cobrir-defice-menos-11-anos-2078030","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"ADSE","url":"https://www.publico.pt/2024/01/25/economia/noticia/excedentes-acumulados-adse-permitem-cobrir-defice-menos-11-anos-2078030"},"id":2078030,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"IP sem datas para a modernização da linha do Douro","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1877566?tp=UH&db=IMAGENS&type=JPG","author":[{"id":366,"nome":"Carlos Cipriano","descricao":"","email":"carlos.cipriano@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"carlos-cipriano","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#110;&#97;&#105;&#114;&#112;&#105;&#99;&#46;&#115;&#111;&#108;&#114;&#97;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/01/25/local/noticia/ip-datas-modernizacao-linha-douro-2077928","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Comboios","url":"https://www.publico.pt/2024/01/25/local/noticia/ip-datas-modernizacao-linha-douro-2077928"},"id":2077928,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Presidente da Câmara de Vila Real f
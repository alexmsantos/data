rder no ecrã gigante do Palais des Festivals, o ano de “Wild at Heart” de Lynch. Se não fossem os filmes não saberia o que estaria a fazer aqui – e isso é verdade para o bigger picture: sem os filmes não saberia o que fazer.","email":"vasco.camara@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":" em Veneza","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"vasco-camara","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"vasco-camara-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1747423?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Cannes","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#97;&#109;&#97;&#99;&#46;&#111;&#99;&#115;&#97;&#118;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/08/27/culturaipsilon/noticia/veneza-nome-pai-filhos-espirito-santo-encontrar-graca-2145087","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Cinema","url":"https://www.publico.pt/2025/08/27/culturaipsilon/noticia/veneza-nome-pai-filhos-espirito-santo-encontrar-graca-2145087"},"id":2145087,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Open dos EUA: Novak Djokovic já não é o mesmo, mas ainda ganha","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2025727?tp=UH&db=IMAGENS&type=JPG","author":[{"id":141,"nome":"Pedro Keul","descricao":"","email":"pkeul@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":null,"profissaoNaAltura":"","slug":"pedro-keul","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#117;&#101;&#107;&#112;","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"ur
ve a imensa sorte de ver revoluções. Nunca mais parei de fazer do outro mundo o meu. E até aprendi a escrever Saddam em árabe.","email":"slorena@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sofia-lorena","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"sofia-lorena-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739860?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#101;&#114;&#111;&#108;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/27/mundo/noticia/exercito-israelita-mata-seis-palestinianos-operacao-cisjordania-2074924","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Médio Oriente","url":"https://www.publico.pt/2023/12/27/mundo/noticia/exercito-israelita-mata-seis-palestinianos-operacao-cisjordania-2074924"},"id":2074924,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":26,"title":"ONU preocupada com a guerra em Gaza, que pode durar “meses ou anos”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1870056?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3527,"nome":"António Rodrigues","descricao":"","email":"antonio.rodrigues@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"antonio-rodrigues","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"antonio-rodrigues-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1750085?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExterna
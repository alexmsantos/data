a que uso para aperfeiçoar o jornal que todos os dias sai para as bancas.","email":"nsousa@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editor de Desporto","profissaoNaAltura":"Editor de Desporto","slug":"nuno-sousa","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"nuno-sousa-rec.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1265428?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#115;&#117;&#111;&#115;&#110;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/07/18/desporto/noticia/jogos-olimpicos-ja-saltam-barreiras-centro-paris-2098130","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Jogos Olímpicos","url":"https://www.publico.pt/2024/07/18/desporto/noticia/jogos-olimpicos-ja-saltam-barreiras-centro-paris-2098130"},"id":2098130,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"Cristina Planas Leitão deixa de ser co-directora do Teatro Municipal do Porto","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1925462?tp=UH&db=IMAGENS&type=JPG","author":[{"id":12716,"nome":"Daniel Dias","descricao":"","email":"daniel.dias@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"daniel-dias","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#105;&#100;&#46;&#108;&#101;&#105;&#110;&#97;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/07/18/culturaipsilon/noticia/cristina-planas-leitao-deixa-codirectora-teatro-municipal-porto-2098107","comments":0,"spoonsored":false,"isOpinion":fa
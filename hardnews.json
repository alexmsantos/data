a conversa de café</i>.","email":"diogo.oliveira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"diogo-cardoso-oliveira","imagem":{"w":null,"h":null,"largura":1064,"altura":1345,"nome":"WhatsApp Image 2025-04-24 at 20.31.38.jpeg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1995019?tp=UH&db=IMAGENS&type=JPG","is_vertical":true,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#118;&#105;&#108;&#111;&#46;&#111;&#103;&#111;&#105;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/07/07/desporto/noticia/nico-hulkenberg-piloto-certo-podio-errado-2139308","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Fórmula 1","url":"https://www.publico.pt/2025/07/07/desporto/noticia/nico-hulkenberg-piloto-certo-podio-errado-2139308"},"id":2139308,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":28,"title":"Tour: Tim Merlier vence ao sprint 3.ª etapa marcada por quedas<em></em>","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2013624?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"},{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2025/07/07/d
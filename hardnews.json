ic":{"name":"PSP","url":"https://www.publico.pt/2024/10/30/local/noticia/detido-10-carros-incendiados-benfica-2109959"},"id":2109959,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"“O retalho é uma referência na descarbonização”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1949002?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2024/10/29/estudiop/noticia/retalho-referencia-descarbonizacao-2109052","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"APED","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1948973?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2024/10/29/estudiop/noticia/retalho-referencia-descarbonizacao-2109052"},"id":2109052,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Chuvas de Espanha perdem força ao chegar a Portugal já esta quarta-feira","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1951080?tp=UH&db=IMAGENS&type=JPG","author":[{"id":177,"nome":"Andréia Azevedo Soares","descricao":"Nasci no Rio de Janeiro, onde estudei jornalismo na Universidade Federal. Mudei-me para Portugal em 1999. Trabalho no PÚBLICO desde 2000, com duas interrupções – uma para um doutoramento em Comunicação de Ciência no Imperial College, em Londres, e outra para colaborar com a revista científica WHO Bulletin, em Genebra. Já fui estagiária, colaboradora, repórter e sub-editora em diferentes secções deste jornal. A ciência e a literatura são as áreas sobre as quais mais gosto de escrever; fascina-me o modo como o escritor e o cientista se empenham para detalhar e compreender o mundo que nos rodeia, incluindo os seres vivos que nele habitam.\r\n\r\n\r\n\r\n","email":"aasoares@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"andreia-azevedo-soares","imagem":{"w":null,"h":null,"largura":501,"altura":498,"nome":"andreia_soares-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1725026?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#114;&#97;&#111;&#115;&#97;&#97;","contribuicao":"","tipo":"INTERNO"},{"id":3619,"nome":"com Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":null,"profissaoNaAltura":"","slug":"com-lusa","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2024/10/30/azul/noticia/chuva-espanha-perder-intensidade-chegar-portugal-quintafeira-2109958","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Mau tempo","url":"https://www.publico.pt/2024/10/30/azul/noticia/chuva-espanha-perder-intensidade-chegar-portugal-quintafeira-2109958"},"id":2109958,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"Primeiro aviso móvel sobre tempestade em Valência só foi emitido na terça à noite","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1951089?tp=UH&db=IMAGENS&type=JPG","author":[{"id":16362,"nome":"André Certã","descricao":"Estagiário Abril 2024","email":"andre.certa@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista estagiário","profissaoNaAltura":"","slug":"andre-certa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#114;&#101;&#99;&#46;&#101;&#114;&#100;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/30/azul/noticia/aviso-telemoveis-
[{"pos":0,"title":"Sismo de 5,3 abala região do Sul do país","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1933913?tp=UH&db=IMAGENS&type=PNG","author":[{"id":12306,"nome":"Patrícia Jesus","descricao":"","email":"patricia.jesus@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editora executiva","profissaoNaAltura":"","slug":"patricia-jesus","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#117;&#115;&#101;&#106;&#46;&#97;&#105;&#99;&#105;&#114;&#116;&#97;&#112;","contribuicao":"","tipo":"INTERNO"},{"id":5957,"nome":"Sónia Sapage","descricao":"Sou \"jornalista\" desde a escola primária, ainda em Trás-os-Montes. Sem carteira e sem curso, mas com microfones inventados e letras ainda incertas. Acabei por me licenciar em Ciências da Comunicação pela Universidade Nova de Lisboa, turma de 1993/97. Vinte e três anos depois, cheguei ao PÚBLICO, onde fui editora da secção de Política, depois editora-executiva e agora directora-adjunta. Pelo meio, fiz Erasmus, experimentei rádio, televisão e especializei-me em Política, área que comecei a trabalhar na revista Visão. No jornalismo, fui fazendo de tudo. Das crónicas de costume às análises e entrevistas. Da reportagem no fim da rua às incursões jornalísticas pelo estrangeiro. O jornalismo (também o meu) quase nunca se repete.","email":"sonia.sapage@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Directora-adjunta","profissaoNaAltura":"","slug":"sonia-sapage","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"sonia-sapage-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739859?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#1
":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"mariana-oliveira","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#118;&#105;&#108;&#111;&#101;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":1,"url":"/2025/08/06/sociedade/noticia/governo-anuncia-compra-dois-kits-combate-fogos-avioes-c130-2143103","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Incêndios florestais","url":"https://www.publico.pt/2025/08/06/sociedade/noticia/governo-anuncia-compra-dois-kits-combate-fogos-avioes-c130-2143103"},"id":2143103,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":24,"title":"Witkoff e Putin: Kremlin elogia encontro, Casa Branca ameaça com tarifas","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2021047?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3527,"nome":"António Rodrigues","descricao":"","email":"antonio.rodrigues@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"antonio-rodrigues","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"antonio-rodrigues-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1750085?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#117;&#103;&#105;&#114;&#100;&#111;&#114;&#46;&#111;&#105;&#110;&#111;&#116;&#110;&#97;","contribuicao":"","tipo":"INTERNO"},{"id":12789,"nome":"Ivo Neto","descricao":"Cheguei ao jornalismo com idade para ter juízo. Foi em 2016, aos 28 anos. Comecei no JN, na imponente torre da Gonçalo Crist
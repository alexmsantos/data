a do Azul, a nossa secção dedicada à crise climática e sustentabilidade, e aventurei-me num segundo mestrado: Economia e Gestão do Ambiente. Gosto de ouvir e de contar as histórias que ficam à margem e acredito que temos sempre algo a aprender quando encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/07/26/azul/entrevista/nao-agir-deixou-legalmente-aceitavel-decisao-tribunal-onu-dar-impulso-lei-bases-clima-2141791","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/justica-climatica"},"id":2141791,"type":"NOTICIA","isMultipleRubric":true,"tag":{"id":14307,"nome":"Justiça climática","slug":"justica-climatica","tagEn":"","isPrincipal":false,"isVisible":true,"urlRoute":"/tjustica-climatica","isPrincipalParaArtigo":true,"isTimeLine":false,"forcaConteudoAberto":false,"follow_type":null,"descricao":null,"imagem":null,"html":null,"isForForuns":false,"isUsedInForuns":false},"size":30},{"pos":10,"title":"Morreram 121 pessoas por afogamento em Portugal em 2024","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2018536?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"u
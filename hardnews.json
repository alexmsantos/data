iamente naquilo de que mais gosto. Política, viagens, música, ecologia e ambiente são outros temas da minha área de interesses.","email":"dandrade@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"david-andrade","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"david-andrade-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1849770?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Arminia Bielefeld","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#101;&#100;&#97;&#114;&#100;&#110;&#97;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/05/25/desporto/noticia/epoca-united-salva-wembley-dois-adolescentes-2091842","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Futebol internacional","url":"https://www.publico.pt/2024/05/25/desporto/noticia/epoca-united-salva-wembley-dois-adolescentes-2091842"},"id":2091842,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Portugal tem de agir já para eliminar a Hepatite C até 2030","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1910077?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2024/05/24/estudiop/noticia/portugal-agir-ja-eliminar-hepatite-c-ate-2030-2091280","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Abvvie","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1910066?tp=UH&db=IMAGENS","slug":"abvvie","url":""},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2024/05/24/estudiop/noticia/portugal-agir-ja-eliminar-hepatite-c-ate-2030-2091280"},"id":2091280,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

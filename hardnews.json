, invoco o facto de escrever sobre música brasileira há quase duas décadas. Nasci no ano (e no mês) da morte de Carmen Miranda, Agosto de 1955, mas não acho que isso conte para esta história.","email":"nuno.pacheco@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactor-principal","profissaoNaAltura":"Redactor-principal","slug":"nuno-pacheco","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Nuno_Pacheco-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739377?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":"Nuno Pacheco","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#99;&#101;&#104;&#99;&#97;&#112;&#46;&#111;&#110;&#117;&#110;","contribuicao":"texto","tipo":"INTERNO"},{"id":5147,"nome":"João Fazenda","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"joao-fazenda","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"ilustração","tipo":"EXTERNO"}],"shares":39,"url":"/2025/02/16/culturaipsilon/noticia/carlos-paredes-guitarra-povo-voltou-tocar-2122470","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Centenário do nascimento de Carlos Paredes","url":""},"id":2122470,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":27,"title":"Vai chamar-se DAViD: laboratório de digitalização e visualização imersiva de arquivos","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1978131?tp=UH&db=IMAGENS&type=JPG","author":[{"id":12716,"nome":"Daniel Dias","descricao":"","email":"daniel.dias@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAlt
facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editora executiva","profissaoNaAltura":"Editora executiva","slug":"helena-pereira","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"helena-pereira-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739855?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Público","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#101;&#112;&#46;&#97;&#110;&#101;&#108;&#101;&#104;","contribuicao":"","tipo":"INTERNO"},{"id":7253,"nome":"Susana Madureira Martins (Renascença)","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Renascença","profissaoNaAltura":"Renascença","slug":"susana-madureira-martins","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"},{"id":3920,"nome":"Daniel Rocha","descricao":"","email":"drocha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"Fotojornalista","slug":"daniel-rocha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Daniel_Rocha.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737766?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#104;&#99;&#111;&#114;&#100;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":2,"url":"/2023/10/26/sociedade/entrevista/americo-aguiar-cardeal-papa-possibilidades-sao-reais-2067863","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/igreja-catolica"},"id":2067863,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":7,"title":"João Costa: os bons resultados dos alunos na pandemia foram uma “anomalia”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1853648?tp=UH&db=IMAGENS&type=JPG","author":[{"id":843,"nome":"Clara Viana","descricao":"Estou no PÚBLICO desde Setembro de 1989, seis meses antes do primeiro número oficial deste jornal. Comecei no Internacional, a área onde já trabalhava antes, passei depois pela revista de domingo (Magazine primeiro e depois Pública), pelo Local e estou agora na secção Portugal, onde me ocupo sobretudo de assuntos relacionados com a tumultuosa área da Educação. Do meu passado anterior fazem parte dois jornais que já não existem: o veterano Diário de Lisboa — onde iniciei a profissão em 1982, com 23 anos, depois de um estágio que ali realizara em 1980 — e o metéorico Europeu, que ajudei a começar em 1988 e que não chegou a durar um ano. Licenciei-me em Ciências da Comunicação na Universidade Nova de Lisboa. Fiz parte do grupo que estreou este curso, que então se chamava Comunicação Social, no ano lectivo de 1979/80.","email":"cviana@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"clara-viana","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#97;&#105;&#118;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/10/26/sociedade/noticia/joao-costa-bons-resultados-alunos-pandemia-anomalia-2068024","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Educação","url":"https://www.publico.pt/2023/10/26/sociedade/noticia/joao-costa-bons-resultados-alunos-pandemia-anomalia-2068024"},"id":2068024,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":8,"title":"Pizarro recebeu mais perguntas dos deputados do que oito ministros juntos","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1853674?tp=UH&db=IMAGENS&type=JPG","author":[{"id":11322,"nome":"Henrique Pinto de Mesquita","descricao":"Nunca quis ser jornalista. A vontade veio quando li um jornalista norte-americano descrever o seu lema — turn every page — para justificar aos fãs (que lhe atiravam a iminência da sua morte à cara) porque estava a demorar tanto a acabar o último volume de uma biografia. Antes disso, estudei Cultura Europeia, na London School Economics — onde me foquei em Filosofia Europeia e defendi que os memes da internet são uma expressão da pós-modernidade —, e Relações Internacionais, na Lusíada do Porto. Gosto de música, livros e praia — assim como de revistas e jornais que possa levar enroladinhos para lá.","email":"henrique.mesquita@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"henrique-pinto-mesquita","imagem":{"w":null,"h":null,"largura":469,"altura":472,"nome":"fotohenrique.jpeg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1820935?tp=UH&db=IMAGENS&type=JPG","is_vertical":true,"tags":"Francesco Petruccione","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#105;&#117;&#113;&#115;&#101;&#109;&#46;&#101;&#117;&#113;&#105;&#114;&#110;&#101;&#104;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/10/26/politica/noticia/manuel-pizarro-recebeu-perguntas-deputados-oito-ministros-juntos-2068043","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Governo","url":"https://www.publico.pt/2023/10/26/politica/noticia/manuel-pizarro-recebeu-perguntas-deputados-oito-ministros-juntos-2068043"},"id":2068043,"type":"NOTICIA","isMultipleRubric"
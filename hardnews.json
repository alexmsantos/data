nha chegada, a equipa do Local Porto, mas fui sempre participando em outras áreas do jornal, com trabalhos para o P2 ou a Pública (que, entretanto, terminaram), a Fugas ou, mais recentemente, a revista 2.","email":"patricia.carvalho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"patricia-carvalho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Patricia Carvalho.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1381570?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#97;&#118;&#114;&#97;&#99;&#46;&#97;&#105;&#99;&#105;&#114;&#116;&#97;&#112;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/02/sociedade/noticia/governo-aumenta-35-comparticipacao-lares-apoio-domiciliario-2106265","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Idosos","url":"https://www.publico.pt/2024/10/02/sociedade/noticia/governo-aumenta-35-comparticipacao-lares-apoio-domiciliario-2106265"},"id":2106265,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"Judiciária detém ex-procurador Orlando Figueira e entrega-o na cadeia","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1942722?tp=UH&db=IMAGENS&type=JPG","author":[{"id":8402,"nome":"Lusa e PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa-e-publico","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/10/02/sociedade/noticia/judiciaria-detem-exprocurador-orlando-figueira-entregao-cadeira-
ustiça.","email":"meoliveira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"mariana-oliveira","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#118;&#105;&#108;&#111;&#101;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/03/22/sociedade/noticia/tres-anos-subsidio-mensal-pago-inem-bombeiros-duplicou-2126939","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"INEM","url":"https://www.publico.pt/2025/03/22/sociedade/noticia/tres-anos-subsidio-mensal-pago-inem-bombeiros-duplicou-2126939"},"id":2126939,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":6,"title":"Trump admite conflito de interesses de Musk na China e nega acesso a planos de guerra","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1986862?tp=UH&db=IMAGENS&type=JPG","author":[{"id":5600,"nome":"Pedro Guerreiro","descricao":"Sou jornalista do PÚBLICO nos Estados Unidos da América e editor online. Sou autor da newsletter Aqui na América e co-autor do podcast Fogo e Fúria. Fui editor de redes sociais de 2020 a 2023. Formei-me em jornalismo pelo CENJOR e sou licenciado em Estudos Europeus.","email":"pedro.guerreiro@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"nos Estados Unidos","profissaoActual":"Jornalista e Editor Online","profissaoNaAltura":"","slug":"pedro-guerreiro","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"pguerreiro.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1835785?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Tour Challenger ATP","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;
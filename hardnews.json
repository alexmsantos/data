ctualidade desportiva com a avidez e o interesse do dia de estreia. E acompanhado o relançamento do site de Desporto, a criação de canais dedicados (Jogos Olímpicos, Europeu, Mundial de futebol) e a edição de suplementos em papel com a mesma lupa que uso para aperfeiçoar o jornal que todos os dias sai para as bancas.","email":"nsousa@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editor de Desporto","profissaoNaAltura":"Editor de Desporto","slug":"nuno-sousa","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"nuno-sousa-rec.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1265428?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#115;&#117;&#111;&#115;&#110;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/08/27/desporto/noticia/paris-2024-oportunidade-ouro-relancar-desporto-adaptado-2102022","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Jogos Paralímpicos","url":"https://www.publico.pt/2024/08/27/desporto/noticia/paris-2024-oportunidade-ouro-relancar-desporto-adaptado-2102022"},"id":2102022,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Operação israelita matou nove palestinianos na Cisjordânia","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1934278?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/08/28/mundo/noticia/operacao-israelita-matou-nove-palestinianos-cisj
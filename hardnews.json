ents":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Extrema-direita","url":"https://www.publico.pt/2025/05/29/mundo/noticia/europa-acontece-extremadireita-segunda-forca-politica-2134683"},"id":2134683,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Gouveia e Melo apresenta-se ao serviço, agora de olhos postos em Belém","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2003943?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3898,"nome":"Liliana Borges","descricao":"","email":"liliana.borges@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"liliana-pascoal-borges","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"liliana-borges.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1172095?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#103;&#114;&#111;&#98;&#46;&#97;&#110;&#97;&#105;&#108;&#105;&#108;","contribuicao":"","tipo":"INTERNO"}],"shares":127,"url":"/2025/05/29/politica/noticia/gouveia-melo-apresentase-servico-olhos-postos-belem-2134658","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Presidência da República","url":"https://www.publico.pt/2025/05/29/politica/noticia/gouveia-melo-apresentase-servico-olhos-postos-belem-2134658"},"id":2134658,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Com os vinhos COZs, Tiago e António defendem a casta Vital e a serra de Montejunto","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2003381?tp=UH&db=IMAGENS&type=JPG","author":[{"id":633,"nome":"Alexandra Prado Coelho","descricao":"Quando entrei no PÚBLICO, em 1989, entre os estagiários que participaram na aventura da fundação do jornal, queria muito trabalhar na secção Internacional. O mundo estava a mudar, em Berlim o Muro caía, Saddam Hussein invadia o Kuwait , usávamos telex e nem sonhávamos com a Internet. Durante 15 anos viajei pelo Médio Oriente, Rússia, Europa, Afeganistão. Depois, foi tempo de mudar e passei para a Cultura, onde escrevi sobre cinema, teatro, arquitectura, design, literatura. Em 2011 passei a acompanhar a área da alimentação – dos produtores aos restaurantes, das novas tendências gastronómicas à saúde. Porque, se há tema que cruza todos os países, culturas e pessoas, é o da comida.","email":"apc@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"alexandra-prado-coelho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Alexandra Prado Coelho z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1526725?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#99;&#112;&#97;","contribuicao":"texto","tipo":"INTERNO"},{"id":3920,"nome":"Daniel Rocha","descricao":"","email":"drocha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"daniel-rocha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Daniel_Rocha.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737766?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#104;&#99;&#111;&#114;&#100;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":12,"url":"/2025/05/29/fugas/reportagem/vinhos-cozs-tiago-antonio-defendem-casta-vital-serra-montejunto-2134051","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"fugas"},"brand":null,"rubric":{"name":"Reportagem","url":"/especial-vinhos"},"id":2134051,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"<em>Mountainhead</em>: Os quatro cavaleiros do Apocalipse têm os melhores <em>gadgets</em>","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2003688?tp=UH&db=IMAGENS&type=JPG","author":[{"id":136,"nome":"Joana Amaral Cardoso","descricao":"Comecei na secção de Sociedade em 1999, dali fui para o Publico.pt e como colaboradora da revista Pública escrevi sobre tendências sociais e Manias. Em 2006 comecei a debruçar-me sobre uma das áreas que mais me interessam – o audiovisual. Depois, já na secção de Cultura, onde sou hoje jornalista, juntei a isto tudo a cultura popular, o cinema, a moda, o design e interesso-me em aplicar a lente do género a muitos fenómenos.  Em 2009 editei a secção de Televisão da revista TimeOut e mais recentemente dei aulas de Jornalismo de Moda. No Público também editei a secção da Cultura online, fui subeditora da revista Pública e co-editora da revista Fugas. E vejo demasiada televisão.","email":"joana.cardoso@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"joana-amaral-cardoso","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#115;&#111;&#100;&#114;&#97;&#99;&#46;&#97;&#110;&#97;&#111;&#106;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/05/29/culturaipsilon/noticia/mountainhead-quatro-cavaleiros-apocalipse-melhores-gadgets-2134551","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Televisão","url":
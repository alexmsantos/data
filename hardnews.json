pal em Setembro deste ano.","email":"ana.sa.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactora-principal","profissaoNaAltura":"Redactora-principal","slug":"ana-sa-lopes","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"ana-sa-lopes-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739858?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":"Ana Sousa Dias","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#115;&#46;&#97;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/02/25/politica/noticia/antonio-vitorino-perto-candidato-oficial-ps-2123905","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"António Vitorino","url":"https://www.publico.pt/2025/02/25/politica/noticia/antonio-vitorino-perto-candidato-oficial-ps-2123905"},"id":2123905,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

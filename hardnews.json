:null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"","profissaoNaAltura":"","slug":"reuters","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":null,"tipo":"AGENCIAS"},{"id":7325,"nome":"Joana Gonçalves","descricao":"Licenciei-me em Ciências da Comunicação pela Universidade Nova de Lisboa e concluí, em 2018, uma pós-graduação em Jornalismo Multiplataforma. No mesmo ano comecei a trabalhar na Renascença, a redacção onde me formei, e aprendi que também na rádio há lugar para a imagem. Cheguei ao PÚBLICO em 2022.","email":"jmgoncalves@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista multimédia","profissaoNaAltura":"Jornalista multimédia","slug":"joana-goncalves","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#118;&#108;&#97;&#99;&#110;&#111;&#103;&#109;&#106;","contribuicao":null,"tipo":"INTERNO"}],"shares":0,"url":"/2025/06/02/video/greta-thunberg-onze-activistas-caminho-gaza-navio-humanitario-20250602-114957","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Vídeo","url":"/video-"},"id":97042,"type":"VIDEO","isMultipleRubric":false,"tag":null,"size":30},{"pos":7,"title":"Autor das mortes no Centro Ismaili declarado imputável e condenado a 25 anos","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2004978?tp=UH&db=IMAGENS&type=JPG","author":[{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2025/06/02/sociedade/noticia/tribunal-condena-pena-maxima-homem-matou-duas-mulheres-centro-
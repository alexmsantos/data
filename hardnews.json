thor":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":57,"url":"/2025/02/16/desporto/noticia/ciclistas-rui-oliveira-ivo-oliveira-vicecampeoes-europa-madison-2122757","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Ciclismo","url":"https://www.publico.pt/2025/02/16/desporto/noticia/ciclistas-rui-oliveira-ivo-oliveira-vicecampeoes-europa-madison-2122757"},"id":2122757,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":10,"title":"Carlos Paredes: a guitarra de um povo voltou a tocar","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1978040?tp=UH&db=IMAGENS&type=JPG","author":[{"id":52,"nome":"Nuno Pacheco","descricao":"Integrar em 1989 a equipa fundadora do PÚBLICO, após oito anos no Expresso, foi um dos grandes desafios da minha vida, faltavam ainda uns anos para o advento revolucionário da Internet. Que não mudou a essência do que acredito que deve ser o jornalismo: uma mistura de ética, arte e busca incessante do que é novo. E isso é inseparável do tratamento dado à palavra, na forma como se escreve uma história, se formula uma ideia, se incentiva um debate. Por isso sou defensor acérrimo da diversidade da língua portuguesa, nas suas riquíssimas variantes, e adversário do acordo ortográfico de 1990. Em apoio desta posição, invoco o facto de escrever sobre música brasileira há quase duas décadas. Nasci no ano (e no mês) da morte de Carmen Miranda, Agosto de 1955, mas não acho que isso conte para esta história.","email":"nuno.pacheco@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactor-principal","profissaoNaAltura":"Redactor-principal","slug":"nuno-
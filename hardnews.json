o a secção de Política e tento manter debaixo de olho o panorama dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"texto","tipo":"INTERNO"},{"id":217,"nome":"Samuel Alemão","descricao":"","email":"smalemao@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"samuel-alemao","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Samuel Alemao.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1990666?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#97;&#109;&#101;&#108;&#97;&#109;&#115;","contribuicao":"texto","tipo":"INTERNO"},{"id":3983,"nome":"Rui Gaudêncio","descricao":"","email":"rgaudencio@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"rui-gaudencio","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#105;&#99;&#110;&#101;&#100;&#117;&#97;&#103;&#114;","contribuicao":"fotografia","tipo":"INTERNO"}],"shares":0,"url":"/2025/09/28/politica/entrevista
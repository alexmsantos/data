[{"pos":0,"title":"Crescimento económico em risco por falta de trabalhadores, alerta OCDE","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2014361?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1053,"nome":"Sérgio Aníbal","descricao":"Sou jornalista desde 1998, tendo começado pouco tempo depois de ter concluído a minha licenciatura em economia no ISEG. Nesse ano, fiz um estágio não-remunerado de três meses no Semanário Económico, jornal onde acabei por ficar durante seis anos, como redactor, primeiro, e como editor da secção de Economia, no final. Passei depois pelo Jornal de Negócios e pelo Diário de Notícias, chegando finalmente ao PÚBLICO em 2006. Sempre a cobrir temas económicos. Nos últimos anos, a crise financeira internacional e a crise da dívida soberana europeia têm ocupado grande parte do meu tempo e atenção.","email":"sergio.anibal@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"sergio-anibal","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sergio_Anibal.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737905?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#108;&#97;&#98;&#105;&#110;&#97;&#46;&#111;&#105;&#103;&#114;&#101;&#115;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/07/09/economia/noticia/crescimento-economico-risco-falta-trabalhadores-alerta-ocde-2139660","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Trabalho e emprego","url":"https://www.publico.pt/2025/07/09/economia/noticia/crescimento-economico-risco-falta-trabalhadores-alerta-ocde-2139660"},"id":2139660,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"D
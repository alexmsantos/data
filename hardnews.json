iei-me em Ciências da Comunicação na Universidade Nova de Lisboa. Fiz parte do grupo que estreou este curso, que então se chamava Comunicação Social, no ano lectivo de 1979/80.","email":"cviana@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"clara-viana","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#97;&#105;&#118;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":128,"url":"/2024/02/13/sociedade/noticia/ine-contraria-ministerio-educacao-confirma-agravamento-abandono-escolar-2080236","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Educação","url":"https://www.publico.pt/2024/02/13/sociedade/noticia/ine-contraria-ministerio-educacao-confirma-agravamento-abandono-escolar-2080236"},"id":2080236,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Diogo Ribeiro, Gabriel Lopes e Mariana Cunha avançam para meias-finais de natação","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1882960?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/02/14/desporto/noticia/diogo-ribeiro-gabriel-lopes-mariana-cunha-avancam-meiasfinais-natacao-2080278","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Natação","url":"https://www.publico.pt/2024/02/14/desporto/noticia/diogo-ribeiro-gabriel-lopes-mariana-cunha-avancam-meiasfinais-natacao-2080278"},"id":2080278,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":14,"title":"R
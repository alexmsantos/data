rl":"/2025/02/16/impar/noticia/faltou-realeza-glamour-britanica-passadeira-vermelha-bafta-2122760","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"impar"},"brand":null,"rubric":{"name":"Moda","url":"https://www.publico.pt/2025/02/16/impar/noticia/faltou-realeza-glamour-britanica-passadeira-vermelha-bafta-2122760"},"id":2122760,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Austrália proíbe estrangeiros de comprar casas no país para conter preços","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1978144?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2025/02/16/mundo/noticia/australia-proibe-estrangeiros-comprar-casas-pais-conter-precos-2122759","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Habitação","url":"https://www.publico.pt/2025/02/16/mundo/noticia/australia-proibe-estrangeiros-comprar-casas-pais-conter-precos-2122759"},"id":2122759,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"Semana AXN","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1953075?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"https://www.publico.pt/estudiop/semana-axn","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Estudio P","url":"/estudiop"},"id":26783,"type":"EXTERNOS","isMultipleRubric":false,"tag":null,"size":30},{"pos":19,"title":"Frente ao Farense, o FC Porto deu a Pinto da Costa a homenagem que ele ia querer","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1978247?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3960,"nome":"Jorge Miguel Matias","descricao":"Sou licenciado em Ciências da Comunicação pela Faculdade de Ciências Sociais e Humanas da Universidade Nova de Lisboa e frequentei ainda uma formação adicional em Jornalismo promovida pelo semanário Expresso. Tenho trabalhado na área do desporto desde o início da minha actividade profissional, primeiro na já extinta Gazeta dos Desportos, depois no PÚBLICO, projecto que abracei de forma definitiva em 1998, embora já antes tivesse uma colaboração regular com o jornal. Actualmente, exerço funções de editor. À margem do desporto, colaborei também com revistas como a \"Rotas e Destinos\" ou a \"National Geographic\".","email":"jmatias@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editor de Desporto","profissaoNaAltura":"Editor de Desporto","slug":"jorge-miguel-matias","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"jorgemiguelmatias-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1758433?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#105;&#116;&#97;&#109;&#106;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/02/16/desporto/cronica_de_jogo/fc-porto-deu-pinto-costa-homenagem-ia-querer-2122768","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Crónica de jogo","url":"/fc-porto"},"id":2122768,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":20,"title":"Pinto da Costa: 15.344 dias em Portugal e no mundo","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1978101?tp=UH&db=IMAGENS&type=JPG","author":[{"id":107,"nome":"Paulo Curado","descricao":"Entrei no PÚBLICO pouco depois do seu nascimento, com apenas 18 anos. Posicionei-me no fim de uma lista de candidatos a um lugar na secção de Agenda do jornal. Depois de uns meses a ganhar experiência, acabei por ficar. Os anos voaram e aqui cresci entre grandes nomes do jornalismo português. O seu profissionalismo, rigor e dedicação foram a minha escola. Ajudei a fundar uma secção pioneira no jornalismo português, na altura denominada Rádio & Televisão, que seria precursora dos actuais Media. Em determinada altura, aventurei-me no Desporto, uma secção com grande variedade temática e inimiga das rotinas. Sou licenciado em História pela Universidade Nova de Lisboa, onde frequento o mestrado em História Contemporânea e frequentei o curso de comunicação social no CENJOR","email":"paulo.curado@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"paulo-curado","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Paulo_Curado.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737881?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#97;&#114;&#117;&#99;&#46;&#111;&#108;&#117;&#97;&#112;","contribuicao":"","tipo":"INTERNO"}],"shares":33,"url":"/2025/02/16/desporto/noticia/pinto-costa-15344-dias-portugal-mundo-2122727","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Futebol nacional","url":"https://www.publico.pt/2025/02/16/desporto/noticia/pinto-costa-15344-dias-portugal-mundo-2122727"},"id":2122727,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":21,"title":"Pinto da Costa: o homem que não mandou no FC Porto para ser “bom rapaz”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1953657?tp=UH&db=IMAGENS&type=JPG","author":[{"id":953,"nome":"Manuel Carvalho","descricao":"Nasci em Alijó, Alto Douro, em 1965, estudei na extinta Escola Normal do Porto, fui professor durante dois anos e fiz parte do grupo de estagiários que integraram a primeira redacção do PÚBLICO, no final de 1989. Durante quase uma década trabalhei na área da Economia, tendo recebido vários prémios de jornalismo desta área. Neste período fiz estágios e programas de formação em Portugal, Japão (International Studies and Training), Estados Unidos (German Marshall Fund) e Bélgica e escrevi, em autoria exclusiva ou em co-autoria, os meus primeiros livros sobre uma das minhas paixões pessoais: o Douro e a memória do vinho do Porto. Fui grande repórter do Diário Económico e regressei ao PÚBLICO em 1999. Aí, fui grande repórter, lancei a Fugas em parceria com o nosso ex-colega David Lopes Ramos e entrei na direcção do jornal em 2000. Entretanto, estudei Direito na Faculdade de Direito da Universidade do Porto e História na Faculdade de Letras, onde me licenciei. Continuei a escrever sobre vinho (a enofilia é endémica nos durienses), fiz as eleições do Brasil desde o primeiro triunfo de Lula, em 2002. Em 2013 saí da direcção do Público e, como Redactor Principal, estreei a “Memória Futura”, a minha coluna de opinião semanal. E continuei a escrever sobre tudo um pouco, principalmente nas áreas da economia associadas ao desenvolvimento regional, à indústria e à agricultura. Pelo meio, recebi o Prémio Gazeta de Imprensa, em 2014, por uma série de reportagens sobre a I Guerra Mundial em Moçambique que fiz na companhia do fotojornalista Manuel Roberto. Essas reportagens dariam mais tarde lugar à edição do livro “A Guerra que Portugal Quis Esquecer” (Ed. Porto Editora). Em Julho de 2018 assumi o desafio de dirigir a mais completa, diversificada e talentosa redacção do país: a do meu jornal de sempre.","email":"manuel.carvalho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactor principal","profissaoNaAltura":"","slug":"manuel-carvalho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"manuel-carvalho.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1977383?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#97;&#118;&#114;&#97;&#99;&#46;&#108;&#101;&#117;&#110;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":345,"url":"/2025/02/15/desporto/noticia/pinto-costa-homem-nao-mandou-fc-porto-bom-rapaz-2111129","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"FC Porto","url":"https://www.publico.pt/2025/02/15/desporto/noticia/pinto-costa-homem-nao-mandou-fc-porto-bom-rapaz-2111129"},"id":2111129,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":22,"title":"Cortejo fúnebre de Pinto da Costa passa pelo Estádio do Dragão após a missa","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1978263?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"},{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":82,"url":"/2025/02/16/desporto/noticia/cortejo-funebre-pinto-costa-passa-estadio-dragao-apos-missa-2122765","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Porto","url":"https://www.publico.pt/20
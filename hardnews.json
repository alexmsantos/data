author":[{"id":3527,"nome":"António Rodrigues","descricao":"","email":"antonio.rodrigues@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"antonio-rodrigues","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"antonio-rodrigues-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1750085?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#117;&#103;&#105;&#114;&#100;&#111;&#114;&#46;&#111;&#105;&#110;&#111;&#116;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":75,"url":"/2023/12/20/mundo/noticia/filhos-imigrantes-franca-vao-dificuldade-obter-nacionalidade-francesa-2074317","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Imigração","url":"https://www.publico.pt/2023/12/20/mundo/noticia/filhos-imigrantes-franca-vao-dificuldade-obter-nacionalidade-francesa-2074317"},"id":2074317,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":27,"title":"Comissão Europeia quer reduzir protecção do lobo, mas falta fundamento científico","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1868714?tp=UH&db=IMAGENS&type=JPG","author":[{"id":227,"nome":"Clara Barata","descricao":"Gosto de compreender como funciona o mundo. Numa célula, de me espantar como uma máquina biológica esconde a centelha que a transforma numa coisa viva. Nas manobras de bastidores de uma campanha de um político que quer ser eleito Presidente da República, descobre-se um homem e uma teia de interesses tão mais vasta que ele e muito mais que isso: as aspirações, os traumas, os preconceitos e a cultura de um povo que pode ser muito diferente do nosso. Os meus interesses são variados, da ciência à política internacional, ambiente ou tendências sociais, na Europa, nos EUA. Um ponto comum entre eles é sempre a vontade de querer saber como funcionam as coisas — e aplicar uma atitude que aprendi a assumir fazendo jornalismo de ciência, que é não ter medo de fazer perguntas, mesmo que pareçam básicas.","email":"clara.barata@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"clara-barata","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Clara_Barata.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737759?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#97;&#114;&#97;&#98;&#46;&#97;&#114;&#97;&#108;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/20/azul/noticia/comissao-europeia-quer-reduzir-proteccao-lobo-falta-fundamento-cientifico-2074364","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Lobo","url":"https://www.publico.pt/2023/12/20/azul/noticia/comissao-europeia-quer-reduzir-proteccao-lobo-falta-fundamento-cientifico-2074364"},"id":2074364,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":28,"title":"Pedro Nuno e Costa: entre o risco de uma liderança bicéfala e a entreajuda\r\n","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1868394?tp=UH&db=IMAGENS&type=JPG","author":[{"id":157,"nome":"Maria Lopes","descricao":"Entrei para o PÚBLICO, vinda da universidade, quando se vivia a excitação das vésperas da primeira Exposição Mundial em Portugal. Calcorreei a Expo-98 de fio a pavio, de caderno, caneta e, por vezes, de Polaroid na mão. Esses poucos meses ensinaram-me a conjugar o verbo tão português “desenrascar-se”.\r\nNestes mais de 20 anos, passei depois pelas secções Local, Media (por duas vezes) e Economia. Estive no lançamento do suplemento Digital – que apesar do nome existiu apenas em papel – e pertenci à equipa do irreverente projecto Sexta, o jornal gratuito que o PÚBLICO partilhou com A Bola, em 2007 e 2008. Em simultâneo, durante dez anos, escrevi também para o Motores (da Fugas), vivendo os automóveis sob uma perspectiva menos técnica e mais prazenteira. Desde 2009 que integro a secção de Política e tento manter debaixo de olho o panorama dos media. \r\n\r\n","email":"maria.lopes@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"maria-lopes","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Maria_C_Lopes.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737865?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#112;&#111;&#108;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/20/politica/noticia/pedro-nuno-costa-risco-lideranca-bicefala-entreajuda-2074290","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"PS","url":"https://www.publico.pt/2023/12/20/politica/noticia/pedro-nuno-costa-risco-lideranca-bicefala-entreajuda-2074290"},"id":2074290,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Montenegro pede que se comparem anúncios com obras de Pedro Nuno Santos como ministro","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1868622?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizac
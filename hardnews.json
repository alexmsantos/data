 este curso, que então se chamava Comunicação Social, no ano lectivo de 1979/80.","email":"cviana@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"clara-viana","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#97;&#105;&#118;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/08/16/sociedade/noticia/menos-16-mil-professores-vao-estar-escola-nova-20242025-2101059","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Educação","url":"https://www.publico.pt/2024/08/16/sociedade/noticia/menos-16-mil-professores-vao-estar-escola-nova-20242025-2101059"},"id":2101059,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":19,"title":"Os filhos do Sucupira, o maior mercado informal de Cabo Verde","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1932101?tp=UH&db=IMAGENS&type=JPG","author":[{"id":90,"nome":"Lusa","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"lusa","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":"","tipo":"AGENCIA"}],"shares":0,"url":"/2024/08/17/mundo/reportagem/filhos-sucupira-maior-mercado-informal-cabo-verde-2101072","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/cabo-verde"},"id":2101072,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":20,"title":"Reportagem do PÚBLICO é transmitida na RTP3 este fim-de-semana","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1932074?tp=UH&db=IMAGENS&type=JPG","author":[{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":
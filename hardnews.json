cricao":"","email":"andre.vieira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"andre-vieira","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#105;&#118;&#46;&#101;&#114;&#100;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/08/22/local/noticia/essencia-porto-compilada-dicionario-sentimental-2144652","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Memória","url":"https://www.publico.pt/2025/08/22/local/noticia/essencia-porto-compilada-dicionario-sentimental-2144652"},"id":2144652,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"Lucélia Santos: “A floresta vai parar de respirar, ela vai morrer”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/2024544?tp=UH&db=IMAGENS&type=JPG","author":[{"id":633,"nome":"Alexandra Prado Coelho","descricao":"Quando entrei no PÚBLICO, em 1989, entre os estagiários que participaram na aventura da fundação do jornal, queria muito trabalhar na secção Internacional. O mundo estava a mudar, em Berlim o Muro caía, Saddam Hussein invadia o Kuwait , usávamos telex e nem sonhávamos com a Internet. Durante 15 anos viajei pelo Médio Oriente, Rússia, Europa, Afeganistão. Depois, foi tempo de mudar e passei para a Cultura, onde escrevi sobre cinema, teatro, arquitectura, design, literatura. Em 2011 passei a acompanhar a área da alimentação – dos produtores aos restaurantes, das novas tendências gastronómicas à saúde. Porque, se há tema que cruza todos os países, culturas e pessoas, é o da comida.","email":"apc@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"alexandra-prado-coelho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Alexandra Prado Coelho z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1526725?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#99;&#112;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/08/22/culturaipsilon/entrevista/lucelia-santos-floresta-vai-parar-respirar-vai-morrer-nao-alarmismo-2142418","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/teatro"},"id":2142418,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":17,"title":"Pedro Azevedo: o rapaz do cubo mágico","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2024132?tp=UH&db=IMAGENS&type=JPG","author":[{"id":4482,"nome":"Alexandra Couto","descricao":"Jornalista","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"alexandra-couto","imagem":{"w":null,"h":null,"largura":463,"altura":539,"nome":"Alexandra Couto.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1675788?tp=UH&db=IMAGENS&type=PNG","is_vertical":true,"tags":null,"translations":null},"hasImage":true,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"},{"id":3917,"nome":"Nelson Garrido","descricao":"Nasci em Vila Nova de Gaia, Portugal. Em 1996 concluí o bacharelato em Tecnologias da Comunicação Audiovisual. No mesmo ano, frequentei uma formação avançada em fotografia na escola Karel de Grote-Hogescholl Antwerpen, na Bélgica, e fiz um estágio em fotografia digital e de grande formato no Studio Brison, também na Bélgica. Em Portugal, trabalhei em regime de freelancer com várias revistas, na área da fotografia de reportagem e de arquitectura. Em 2000, concluí a licenciatura em Comunicação Social e em 2005 a Licenciatura em Fotografia. Em 2005, ganhei três menções honrosas no concurso de fotojornalismo da revista Visão e venci a categoria de fotografia de arquitectura do Euro Press Photo Awards da Fuji Film. Fui premiado no Prémio Estação Imagem|Mora em 2010 e 2011. Desde 1999, sou fotojornalista no PÚBLICO.","email":"ngarrido@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"nelson-garrido","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Nelson_Garrido.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737874?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#105;&#114;&#114;&#97;&#103;&#110;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/08/22/impar/noticia/pedro-azevedo-rapaz-cubo-magico-2144255","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":{"name":"impar"},"brand":null,"rubric":{"name":"Protagonista","url":"https://www.publico.pt/2025/08/22/impar/noticia/pedro-azevedo-rapaz-cubo-magico-2144255"},"id":2144255,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":18,"title":"Tatiana Faia: “Sou uma pessoa obsessiva”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2024307?tp=UH&db=IMAGENS&type=JPG","author":[{"id":973,"nome":"PÚBLICO","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"publico","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2025/08/22/impar/noticia/tatiana-faia-pessoa-obsess
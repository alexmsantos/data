er.","email":"vasco.camara@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"vasco-camara","imagem":{"w":null,"h":null,"largura":216,"altura":216,"nome":"vasco-camara-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1747423?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Cannes","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#97;&#109;&#97;&#99;&#46;&#111;&#99;&#115;&#97;&#118;","contribuicao":null,"tipo":"INTERNO"},{"id":3925,"nome":"Joana Bourgard","descricao":"Sou jornalista desde 2007 e, ao longo destes anos de profissão, é a terceira vez que estou no PÚBLICO. Comecei como estagiária, na secção de Fotografia do jornal. Prossegui fotojornalismo no Jornal de Notícias, no Porto, onde fiquei três anos. Foi nessa altura que apareceram os smartphones e as primeiras máquinas fotográficas digitais com a possibilidade de captar imagem em movimento. Tornei-me jornalista multimédia e regressei para Lisboa para fazer parte da equipa de vídeo do PÚBLICO, em 2010. Passado quatro anos juntei-me à equipa multimédia da Rádio Renascença. Por fim, 15 anos depois do estágio no PÚBLICO, regressei em 2022 para editar a secção de Multimédia. Em 2007 fiz parte fundação da <a href=\"https://wilder.pt\">Wilder.pt</a>, um projecto de jornalismo independente dedicado à natureza e biodiversidade.","email":"joana.bourgard@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Editora de Multimédia","profissaoNaAltura":"Editora de Multimédia","slug":"joana-bourgard","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Joana_Bourgard.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737812?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#100;&#114;&#97;&#103;&#114;&#117;&#111;&#98;&#46;&#97;&#110;&#97;&#111;&#106;","contribuicao":null,"tipo":"INTERNO"}],"shares":0,"url":"/2025/01/23/culturaipsilon/video/nomeacoes-oscares-agarraram-perturbante-apareceu-ecras-20250123-190136","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Vídeo","url":"/video-"},"id":96711,"type":"VIDEO","isMultipleRubric":false,"tag":null,"size":30},{"pos":29,"title":"Onde é que os dinossauros nasceram? Os cientistas poderão ter uma resposta","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1972716?tp=UH&db=IMAGENS&type=JPG","author":[{"id":4612,"nome":"Will Dunham","descricao":"Reuters","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Reuters","profissaoNaAltura":"","slug":"will-dunham-reuters","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2025/01/25/ciencia/noticia/onde-dinossauros-nasceram-cientistas-poderao-resposta-2119934","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Paleontologia ","url":"https://www.publico.pt/2025/01/25/ciencia/noticia/onde-dinossauros-nasceram-cientistas-poderao-resposta-2119934"},"id":2119934,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

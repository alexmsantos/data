ago@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"david-santiago","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"David Santiago.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1765612?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#103;&#97;&#105;&#116;&#110;&#97;&#115;&#46;&#100;&#105;&#118;&#97;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":53,"url":"/2024/01/28/politica/noticia/albuquerque-continua-liderar-psdmadeira-fala-segundafeira-representante-republica-2078414","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Madeira","url":"https://www.publico.pt/2024/01/28/politica/noticia/albuquerque-continua-liderar-psdmadeira-fala-segundafeira-representante-republica-2078414"},"id":2078414,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":14,"title":"<em>Seu Nome Era Gisberta</em>: uma “viagem imersiva” pela vida de um “marco nacional”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1878135?tp=UH&db=IMAGENS&type=JPG","author":[{"id":17847,"nome":"Ana Margarida Alves","descricao":"","email":"ana.alves@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"ana-margarida-alves","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#118;&#108;&#97;&#46;&#97;&#110;&#97;","contribuicao":"","tipo":"EXTERNO"}],"shares":0,"url":"/2024/01/29/p3/noticia/nome-gisberta-viagem-imersiva-vida-marco-nacional-2077799","comments":0,"spoonsored":false,"isOpinion":false,"homeBran
ente dedicado à natureza e biodiversidade.","email":"joana.bourgard@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Editora de Multimédia","profissaoNaAltura":"Editora de Multimédia","slug":"joana-bourgard","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Joana_Bourgard.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737812?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#100;&#114;&#97;&#103;&#114;&#117;&#111;&#98;&#46;&#97;&#110;&#97;&#111;&#106;","contribuicao":null,"tipo":"INTERNO"},{"id":17569,"nome":"Mariana Godet","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":null,"profissaoActual":"Jornalista de Multimédia","profissaoNaAltura":"Jornalista de Multimédia","slug":"mariana-godet","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"","contribuicao":null,"tipo":"INTERNO"}],"shares":0,"url":"/2023/09/29/economia/video/associacoes-preparam-protesto-habitacao-nao-resolve-crise-nao-ha-vontade-politica-20230929-121113","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Vídeo","url":"/video-"},"id":95645,"type":"VIDEO","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Como preservar as peças favoritas do <em>closet</em> o ano inteiro","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1842892?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2023/09/27/estudiop/noticia/preservar-pecas-favoritas-closet-ano-inteiro-2062956","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Hotpoint","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1712812?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{
[{"pos":0,"title":"Há mais oferta nas telecomunicações. Como mudo de operador?","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1957484?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1083,"nome":"Ana Brito","descricao":"","email":"ana.brito@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"ana-brito","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#116;&#105;&#114;&#98;&#46;&#97;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/11/23/economia/noticia/ha-oferta-telecomunicacoes-mudo-operador-2113019","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Perguntas e respostas","url":"https://www.publico.pt/2024/11/23/economia/noticia/ha-oferta-telecomunicacoes-mudo-operador-2113019"},"id":2113019,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"Da jardinagem ao armazém: como o Incorpora traçou um caminho de inclusão e de sucesso","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1955995?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"https://www.publico.pt/estudiop/conteudo_patrocinado/inclusao-sucesso-incorpora","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Estudio P","url":"/estudiop"},"id":26821,"type":"EXTERNOS","isMultipleRubric":false,"tag":null,"size":30},{"pos":2,"title":"“O que é que nós queremos? ‘<em>Trillions, not billions</em>!’”, ouviu-se na COP29","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1957572?tp=UH&db=IMAGENS&type=JPG","author":[{"id":227,"nome":"Clara Barata","descricao":"Gosto de compreender como funciona o mundo. Numa célula, de me espantar como uma máquina biológica esconde a centelha que a transforma numa coisa viva. Nas manobras de bastidores de uma campanha de um político que quer ser eleito Presidente da República, descobre-se um homem e uma teia de interesses tão mais vasta que ele e muito mais que isso: as aspirações, os traumas, os preconceitos e a cultura de um povo que pode ser muito diferente do nosso. Os meus interesses são variados, da ciência à política internacional, ambiente ou tendências sociais, na Europa, nos EUA. Um ponto comum entre eles é sempre a vontade de querer saber como funcionam as coisas — e aplicar uma atitude que aprendi a assumir fazendo jornalismo de ciência, que é não ter medo de fazer perguntas, mesmo que pareçam básicas.","email":"clara.barata@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"em Bacu","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"clara-barata","imagem":{"w":null,"h":null,"largura":368,"altura":331,"nome":"_RG__foto_Clara_Barata_DSCF4152-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1926383?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#97;&#114;&#97;&#98;&#46;&#97;&#114;&#97;&#108;&#99;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/11/23/azul/reportagem/queremos-trillions-not-billions-ouviuse-cop29-2113046","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reportagem","url":"/cop29"},"id":2113046,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":3,"title":"Tensão na COP29: cimeira do clima continua presa em impasse sobre financiamento","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1957670?tp=UH&db=IMAGENS&type=JPG","author":[{"id":6488,"nome":"Aline Flor","descricao":"Natural do Rio de Janeiro, vivo em Portugal há mais de 25 anos. Criei raízes. Fui directora do Jornal Universitário do Porto, passei pela Ciência e Tecnologia no Canal Superior, editei notícias do portal AEIOU.pt. Fiz um mestrado em Cinema Documental que me abriu os olhos (e os ouvidos) para a riqueza de cada encontro com o outro. Cheguei ao PÚBLICO para integrar a equipa de áudio, onde produzi podcasts em áreas desde a igualdade de género às questões europeias. Hoje faço parte do Azul, a nossa secção dedicada à crise climática e sustentabilidade. Gosto de ouvir e de contar as histórias que ficam à margem e acredito que temos sempre algo a aprender quando encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"},{"id":1026,"nome":"Agências","descricao":"","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"agencias","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"AGENCIAS"}],"shares":0,"url":"/2024/11/23/azul/noticia/cimeira-clima-bacu-presa-impasse-financiamento-2113080","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"COP29","url":"https://www.publico.pt/2024/11/23/azul/noticia/cimeira-clima-bacu-presa-impasse-financiamento-2113080"},"id":2113080,"type":"NOTICIA","i
 emprego!”, de Elisabete Sá, Raquel Almeida Correia e Ana Rute Silva.","email":"luis.villalobos@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"luis-villalobos","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Luis Villalobos.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1527219?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":",Sistema de gerenciamento de conteúdo","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#111;&#98;&#111;&#108;&#97;&#108;&#108;&#105;&#118;&#46;&#115;&#105;&#117;&#108;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/05/28/economia/noticia/modelo-fraudes-fizeram-disparar-despesa-apoio-mobilidade-acores-2133615","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Aviação","url":"https://www.publico.pt/2025/05/28/economia/noticia/modelo-fraudes-fizeram-disparar-despesa-apoio-mobilidade-acores-2133615"},"id":2133615,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":16,"title":"ERC já recebeu centenas de queixas sobre vídeo antiaborto de Miguel Milhão","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/2003709?tp=UH&db=IMAGENS&type=JPG","author":[{"id":14261,"nome":"Carolina Amado","descricao":"","email":"carolina.amado@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"carolina-amado","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#100;&#97;&#109;&#97;&#46;&#97;&#110;&#105;&#108;&#111;&#114;&#97;&#99;","contribuicao":"","tipo":"INTERNO"},{"id":90,"nome":"Lusa","descricao":"","email":"","
eresse do dia de estreia. E acompanhado o relançamento do site de Desporto, a criação de canais dedicados (Jogos Olímpicos, Europeu, Mundial de futebol) e a edição de suplementos em papel com a mesma lupa que uso para aperfeiçoar o jornal que todos os dias sai para as bancas.","email":"nsousa@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editor de Desporto","profissaoNaAltura":"Editor de Desporto","slug":"nuno-sousa","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"nsousa.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1951120?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Liga Europa da UEFA","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#115;&#117;&#111;&#115;&#110;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/08/16/desporto/noticia/primeira-supertaca-franz-beckenbauer-vai-munique-2144151","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Futebol internacional","url":"https://www.publico.pt/2025/08/16/desporto/noticia/primeira-supertaca-franz-beckenbauer-vai-munique-2144151"},"id":2144151,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30}]

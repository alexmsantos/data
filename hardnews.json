doras.","email":"teresa.serafim@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"  em Mons","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"teresa-serafim","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Teresa Serafim.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1686947?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#109;&#105;&#102;&#97;&#114;&#101;&#115;&#46;&#97;&#115;&#101;&#114;&#101;&#116;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/03/24/local/noticia/populismo-cresce-sente-esquecido-podem-municipios-ouvilos-2084529","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"coesão social","url":"https://www.publico.pt/2024/03/24/local/noticia/populismo-cresce-sente-esquecido-podem-municipios-ouvilos-2084529"},"id":2084529,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Siân Sutherland: “Alimentam-nos à força com plástico todos os dias”","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1892721?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1084,"nome":"Nicolau Ferreira","descricao":"Licenciei-me em Biologia, completei o mestrado em Jornalismo Aplicado e aproximei-me da arte através dos estudos do corpo. No PÚBLICO, integrei as secções do Online e da Ciência. Agora, escrevo sobre ambiente no Azul. Gosto de caminhar.","email":"nicolau.ferreira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"nicolau-ferreira","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Nicolau Ferreira.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1686950?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#114;&#101;&#102;&#46;&#117;&#97;&#108;&#111;&#99;&#105;&#110;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/03/23/azul/entrevista/sian-sutherland-alimentamnos-forca-plastico-dias-2083812","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/sustentabilidade"},"id":2083812,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"O Piero della Francesca de Lisboa junta-se aos irmãos em Milão e regressa acompanhado","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1894607?tp=UH&db=IMAGENS&type=JPG","author":[{"id":431,"nome":"Isabel Salema","descricao":"Se pensar no que sou profissionalmente não sei se o que me define mais é a minha formação académica em História de Arte ou o meu percurso jornalístico entre a ciência e a cultura. Acho que o que aprendi nesses primeiros tempos na secção de ciência foi que não havia perguntas que não se fizessem, porque o pior mesmo era continuar sem perceber nada.\r\nFiquei oito anos a fazer jornalismo científico, que adorei, e depois passei para a secção de cultura, onde me tornei editora. Não foi bem um salto, porque trabalhar na área da cultura é trabalhar com a criação, com o novo, com ideias e algo de semelhante se passa na ciência. Depois, editei o caderno P2 e coordenei os suplementos do jornal. Actualmente, sou jornalista da Cultura.","email":"isabel.salema@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"isabel-salema","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Isabel_Salema.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"http
ima","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Antonio Saraiva Lima.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1527215?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":",Norges teknisk-naturvitenskaplige universitet","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#109;&#105;&#108;&#46;&#111;&#105;&#110;&#111;&#116;&#110;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/04/mundo/noticia/brexit-boris-johnson-defende-referendo-convencao-europeia-direitos-humanos-2106610","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Reino Unido","url":"https://www.publico.pt/2024/10/04/mundo/noticia/brexit-boris-johnson-defende-referendo-convencao-europeia-direitos-humanos-2106610"},"id":2106610,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":21,"title":"Abusos de militares diminuíram em Cabo Delgado, mas não acabaram as denúncias","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1943150?tp=UH&db=IMAGENS&type=JPG","author":[{"id":3527,"nome":"António Rodrigues","descricao":"","email":"antonio.rodrigues@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"antonio-rodrigues","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"antonio-rodrigues-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1750085?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#117;&#103;&#105;&#114;&#100;&#111;&#114;&#46;&#111;&#105;&#110;&#111;&#116;&#110;&#97;","contribuicao":"","tipo":"INTERNO"
mas de economia. Nasci e estudei em Coimbra, onde experimentei o jornalismo universitário (jornal A Cabra, revista Via Latina e RUC), e passei pelo Centro Regional de Informação das Nações Unidas, em Bruxelas.","email":"pedro.crisostomo@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":"/autor/pedro-crisostomo","localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"pedro-crisostomo","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Pedro Crisostomo.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530342?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#109;&#111;&#116;&#115;&#111;&#115;&#105;&#114;&#99;&#46;&#111;&#114;&#100;&#101;&#112;","contribuicao":"","tipo":"INTERNO"},{"id":3920,"nome":"Daniel Rocha","descricao":"","email":"drocha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":"/autor/daniel-rocha","localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"daniel-rocha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Daniel_Rocha.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737766?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#104;&#99;&#111;&#114;&#100;","contribuicao":"Fotografia","tipo":"INTERNO"}],"shares":0,"url":"/2023/12/03/economia/entrevista/evasao-fiscal-associada-fluxos-financeiros-ilicitos-2072152","comments":1,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Entrevista","url":"/fisco"},"id":2072152,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":5,"title":"PS, PSD e Chega s
[{"pos":0,"title":"Rendas subiram 7% em 2024, o maior aumento em 30 anos","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1969604?tp=UH&db=IMAGENS&type=JPG","author":[{"id":15415,"nome":"Rafaela Burd Relvas","descricao":"","email":"rafaela.relvas@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"rafaela-burd-relvas","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#97;&#118;&#108;&#101;&#114;&#46;&#97;&#108;&#101;&#97;&#102;&#97;&#114;","contribuicao":"","tipo":"INTERNO"}],"shares":5,"url":"/2025/01/13/economia/noticia/rendas-subiram-7-2024-maior-aumento-30-anos-2118557","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Habitação","url":"https://www.publico.pt/2025/01/13/economia/noticia/rendas-subiram-7-2024-maior-aumento-30-anos-2118557"},"id":2118557,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"Um dia dedicado aos direitos no mundo digital","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1967044?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2025/01/11/estudiop/noticia/dia-dedicado-direitos-mundo-digital-2117338","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Digital Rights Festival","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1969217?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2025/01/11/estudiop/noticia/dia-dedicado-direitos-mundo-digital-2117338"},"id":2117338,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":2,"title":"Vai Rangel à tomada de posse em Moçambique, não o Presidente","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1969669?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1085,"nome":"Bárbara Reis","descricao":"Fui para o Expresso porque queria ser fotógrafa, mas acabei por aprender a escrever. Tinha 17 anos. O PÚBLICO nasceu dois anos depois e os fundadores “trouxeram-me”. Trabalhei no Internacional, cobri eleições, fui à Somália no início da guerra civil e escrevi muito sobre sida. Um dia perguntaram-me se queria ser correspondente em Nova Iorque. Fiquei cinco anos. Foi a era Clinton, mas também a das negociações na ONU entre Portugal e a Indonésia sobre Timor. Passei tantas horas nos corredores do Conselho de Segurança que acabei por aterrar em Díli com passaporte das Nações Unidas. Era o que fazia sentido: trabalhar com Sérgio Vieira de Mello e ver o país nascer. Fui por seis meses para dar formação a jornalistas, mas fiquei dois anos como porta-voz da missão. Regressei ao PÚBLICO em 2002. Editei a Cultura, ajudei a lançar o P2, fui executiva da direcção. Fui directora de 2009 a 2016.","email":"breis@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Redactora principal","profissaoNaAltura":"Redactora principal","slug":"barbara-reis","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"barbara-reis-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739850?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":"Bárbara Reis","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#105;&#101;&#114;&#98;","contribuicao":"","tipo":"COLUNISTAS"}],"shares":0,"url":"/2025/01/13/politica/noticia/vai-rangel-tomada-posse-mocambique-nao-presidente-2118580","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Política","url":"/politica"},"id":2118580,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":3,"title":"Governo acelera devolução de hospitais às misericór
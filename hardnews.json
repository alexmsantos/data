remium":false,"image":"https://imagens.publico.pt/imagens.aspx/1967599?tp=UH&db=IMAGENS&type=JPG","author":[{"id":1080,"nome":"Maria José Santana","descricao":"","email":"maria.jose.santana@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"maria-jose-santana","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#110;&#97;&#116;&#110;&#97;&#115;&#46;&#101;&#115;&#111;&#106;&#46;&#97;&#105;&#114;&#97;&#109;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/01/05/local/noticia/festa-s-goncalinho-aveiro-volta-patrimonio-nacional-documentario-2117584","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Festas populares","url":"https://www.publico.pt/2025/01/05/local/noticia/festa-s-goncalinho-aveiro-volta-patrimonio-nacional-documentario-2117584"},"id":2117584,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Charles Dolan (1926-2024), fundador da HBO e visionário da indústria dos <em>media</em>","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1967636?tp=UH&db=IMAGENS&type=JPG","author":[{"id":136,"nome":"Joana Amaral Cardoso","descricao":"Comecei na secção de Sociedade em 1999, dali fui para o Publico.pt e como colaboradora da revista Pública escrevi sobre tendências sociais e Manias. Em 2006 comecei a debruçar-me sobre uma das áreas que mais me interessam – o audiovisual. Depois, já na secção de Cultura, onde sou hoje jornalista, juntei a isto tudo a cultura popular, o cinema, a moda, o design e interesso-me em aplicar a lente do género a muitos fenómenos.  Em 2009 editei a secção de Televisão da revista TimeOut e mais recentemente dei aulas de Jornalismo de Moda. No Público também editei a secção da Cultura online, fui subeditora da revista Pública e co-editora da revista Fugas. E vejo demasiada televisão.","email":"joana.cardoso@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"joana-amaral-cardoso","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#115;&#111;&#100;&#114;&#97;&#99;&#46;&#97;&#110;&#97;&#111;&#106;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/01/05/culturaipsilon/noticia/criador-hbo-charles-dolan-perdia-aposta-pagava-notas-pedacinhos-2117571","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Obituário","url":"https://www.publico.pt/2025/01/05/culturaipsilon/noticia/criador-hbo-charles-dolan-perdia-aposta-pagava-notas-pedacinhos-2117571"},"id":2117571,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":13,"title":"Dava emprego a um obeso? Estudo quer avaliar atitude dos portugueses face à obesidade","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1967649?tp=UH&db=IMAGENS&type=JPG","author":[{"id":18319,"nome":"Gina Pereira","descricao":"","email":"gina.pereira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":"/autor/gina-pereira","localizacao":"","profissaoActual":"Editora de Sociedade","profissaoNaAltura":"","slug":"gina-pereira","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#101;&#112;&#46;&#97;&#110;&#105;&#103;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2025/01/05/sociedade/noticia/daria-emprego-pessoa-obesa-estudo-quer-avaliar-atitudes-portugueses-face-obesidade-2117583","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Saúde","url":"https://www.publico.pt/2025/01/05/sociedade/noticia/daria-emprego-pessoa-o
jornalista era mesmo o que queria ser. Estou no PÚBLICO desde 2016 a escrever diariamente para a secção de Ciência. Aqui, escuto e reconto algumas das histórias mais bonitas, desafiantes e inspiradoras.","email":"teresa.serafim@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"teresa-serafim","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Teresa Serafim.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1686947?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#109;&#105;&#102;&#97;&#114;&#101;&#115;&#46;&#97;&#115;&#101;&#114;&#101;&#116;","contribuicao":"Texto","tipo":"INTERNO"},{"id":3977,"nome":"Paulo Pimenta","descricao":"","email":"paulo.pimenta@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"","slug":"paulo-pimenta","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#116;&#110;&#101;&#109;&#105;&#112;&#46;&#111;&#108;&#117;&#97;&#112;","contribuicao":"Fotografia","tipo":"INTERNO"},{"id":16232,"nome":"Gabriela Pedro","descricao":"","email":"gabriela.pedro@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Infografia","profissaoNaAltura":"","slug":"gabriela-pedro","imagem":null,"hasImage":false,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#114;&#100;&#101;&#112;&#46;&#97;&#108;&#101;&#105;&#114;&#98;&#97;&#103;","contribuicao":"Infografia","tipo":"INTERNO"},{"id":12031,"nome":"Gabriela Gómez","descricao":"Designer gráfica, mestr
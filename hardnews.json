 gémeos, o Jaime e a Júlia.","email":"helena.pereira@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editora executiva","profissaoNaAltura":"Editora executiva","slug":"helena-pereira","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"helena-pereira-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1739855?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":",Público","translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#114;&#105;&#101;&#114;&#101;&#112;&#46;&#97;&#110;&#101;&#108;&#101;&#104;","contribuicao":"","tipo":"INTERNO"},{"id":17352,"nome":"Tomás Anjinho Chagas (Renascença)","descricao":"Jornalista da Renascença","email":"","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista da Renascença","profissaoNaAltura":"Jornalista da Renascença","slug":"tomas-anjinho-chagas","imagem":null,"hasImage":false,"isExternal":true,"encodedEmail":"","contribuicao":"","tipo":"EXTERNO"},{"id":3920,"nome":"Daniel Rocha","descricao":"","email":"drocha@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Fotojornalista","profissaoNaAltura":"Fotojornalista","slug":"daniel-rocha","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Daniel_Rocha.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/737766?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#97;&#104;&#99;&#111;&#114;&#100;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/10/03/politica/entrevista/marina-goncalves-proposta-irrecusavel-vao-concordar-connosco-irc-irs-j
se climática e sustentabilidade. Gosto de ouvir e de contar as histórias que ficam à margem e acredito que temos sempre algo a aprender quando encaramos o desafio de traduzir no preto e branco dos caracteres as infinitas tonalidades que nos rodeiam.","email":"aline.flor@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"aline-flor","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Aline Flor z.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1530339?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#114;&#111;&#108;&#102;&#46;&#101;&#110;&#105;&#108;&#97;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/11/23/azul/noticia/cop29-aprova-forma-controversa-novo-acordo-financiamento-climatico-2113120","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"COP29","url":"https://www.publico.pt/2024/11/23/azul/noticia/cop29-aprova-forma-controversa-novo-acordo-financiamento-climatico-2113120"},"id":2113120,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Carolina Patrocínio: “Associam-me a um fundamentalismo que nunca foi verdade”","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1955338?tp=UH&db=IMAGENS&type=JPG","author":[{"id":12425,"nome":"Inês Duarte de Freitas","descricao":"","email":"ines.freitas@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"","slug":"ines-duarte-freitas","imagem":{"w":null,"h":null,"largura":344,"altura":320,"nome":"WhatsApp_Image_2023-02-01_at_13.49.04-removebg-preview.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://im
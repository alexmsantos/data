[{"pos":0,"title":"PSD acredita que resolução dos problemas do dia-a-dia vai esvaziar voto de protesto","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1896263?tp=UH&db=IMAGENS&type=JPG","author":[{"id":734,"nome":"Sofia Rodrigues","descricao":"Sou licenciada em Ciências da Comunicação, pela Universidade Autónoma de Lisboa, colaborei no Diário Económico entre 1995 e 1998, data a partir da qual passei a colaborar no PÚBLICO.","email":"srodrigues@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"","profissaoNaAltura":"","slug":"sofia-rodrigues","imagem":{"w":null,"h":null,"largura":null,"altura":null,"nome":"Sofia_Rodrigues.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1202811?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#115;&#101;&#117;&#103;&#105;&#114;&#100;&#111;&#114;&#115;","contribuicao":"","tipo":"INTERNO"},{"id":169,"nome":"Leonete Botelho","descricao":"O jornalismo surgiu como a melhor forma possível - e desejada - de declarar independência ao terminar Direito. O Diário de Coimbra ficava ali na Rua da Sofia, entre o tribunal e o escritório de advogados onde fazia estágio, mas rapidamente o jornalismo se sobrepôs. O prazer da escrita, o resultado imediato, a realidade a acontecer sob os nossos olhos em primeira mão. Viver para contá-la. Voltei à Faculdade quando foi criada a pós-graduação em Direito da Comunicação. Seguiu-se a “tarimba” e o PÚBLICO: três anos correspondente em Coimbra, nove anos na secção de Sociedade no Porto – justiça, migrações, menores, violência doméstica: minorias. Em 2006 rumei a Lisboa e às maiorias: a Política vista da Assembleia da República. Editar foi um desafio que se impôs, outra vez. Mas é na ponta dos dedos, na veia crítica, que o jornalismo fervilha.","email":"lbotelho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Editora de Política","profissaoNaAltura":"","slug":"leonete-botelho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"leonete-botelho-nova.png","legenda":null,"autor":null,"tipo":"PNG","url":"https://imagens.publico.pt/imagens.aspx/1740630?tp=UH&db=IMAGENS&type=PNG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#101;&#116;&#111;&#98;&#108;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/03/30/politica/noticia/psd-acredita-resolucao-problemas-diaadia-vai-esvaziar-voto-protesto-2085306","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"Novo Governo","url":"https://www.publico.pt/2024/03/30/politica/noticia/psd-acredita-resolucao-problemas-diaadia-vai-esvaziar-voto-protesto-2085306"},"id":2085306,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":1,"title":"Halo, uma série do outro mundo, para ver na SkyShowtime","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1888835?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2024/03/21/estudiop/noticia/halo-serie-mundo-skyshowtime-2082484","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"SkyShowTime","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1841267?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2024/03/21/estudiop/noticia/halo-serie-mundo-skyshowtime-2082484"},"id":2082484,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":2,"title":"Fisco prepara terreno para rebater contestação ao IMI das barragens","isPremium":true,"image":"https://imagens.publico.pt/imagens.aspx/1895715?tp=UH&db=IMAGENS&type=
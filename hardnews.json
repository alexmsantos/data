, terminaram), a Fugas ou, mais recentemente, a revista 2.","email":"patricia.carvalho@publico.pt","facebook":null,"googlePlus":null,"twitter":null,"site":null,"url":null,"localizacao":"","profissaoActual":"Jornalista","profissaoNaAltura":"Jornalista","slug":"patricia-carvalho","imagem":{"w":null,"h":null,"largura":225,"altura":225,"nome":"Patricia Carvalho.jpg","legenda":null,"autor":null,"tipo":"JPG","url":"https://imagens.publico.pt/imagens.aspx/1381570?tp=UH&db=IMAGENS&type=JPG","is_vertical":false,"tags":null,"translations":null},"hasImage":true,"isExternal":false,"encodedEmail":"&#116;&#112;&#46;&#111;&#99;&#105;&#108;&#98;&#117;&#112;&#64;&#111;&#104;&#108;&#97;&#118;&#114;&#97;&#99;&#46;&#97;&#105;&#99;&#105;&#114;&#116;&#97;&#112;","contribuicao":"","tipo":"INTERNO"}],"shares":0,"url":"/2024/06/04/sociedade/noticia/crimes-incendio-cairam-menos-terco-ano-2092748","comments":0,"spoonsored":false,"isOpinion":false,"homeBrand":null,"brand":null,"rubric":{"name":"GNR","url":"https://www.publico.pt/2024/06/04/sociedade/noticia/crimes-incendio-cairam-menos-terco-ano-2092748"},"id":2092748,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":11,"title":"Projecto de investigação-acção promove igualdade de género na investigação","isPremium":false,"image":"https://imagens.publico.pt/imagens.aspx/1911909?tp=UH&db=IMAGENS&type=JPG","author":[],"shares":0,"url":"/2024/05/31/estudiop/noticia/projecto-investigacaoaccao-promove-igualdade-genero-investigacao-2091934","comments":0,"spoonsored":true,"isOpinion":false,"homeBrand":null,"brand":{"id":0,"name":"Universidade de Coimbra ","ImageUrl":"https://imagens.publico.pt/imagens.aspx/1774631?tp=UH&db=IMAGENS","slug":null,"url":null},"rubric":{"name":"Conteúdo comercial","url":"https://www.publico.pt/2024/05/31/estudiop/noticia/projecto-investigacaoaccao-promove-igualdade-genero-investigacao-2091934"},"id":2091934,"type":"NOTICIA","isMultipleRubric":false,"tag":null,"size":30},{"pos":12,"title":"Sectores priv